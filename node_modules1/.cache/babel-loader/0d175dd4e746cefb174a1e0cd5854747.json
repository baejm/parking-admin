  </router-link>\r\n        <li>관리기능</li>\r\n        <li>블랙리스트 차량관리</li>\r\n      </ul>\r\n    </div>\r\n    <h1>블랙리스트 차량관리</h1>\r\n    <div class=\"content_wrap\">\r\n      <div class=\"menu_title\">\r\n        <form>\r\n          <span class=\"small_title\">차량번호</span>\r\n          <input\r\n            class=\"search_icon\"\r\n            type=\"text\"\r\n            placeholder=\"차량 번호를 입력해 주세요.\"\r\n            v-model=\"searchLp\"\r\n            maxlength=\"9\"\r\n            value=\"\"\r\n          />\r\n          <button @click.prevent=\"search\" class=\"seach_confirm\">검색</button>\r\n          <button @click.prevent=\"registOpen\" class=\"registerbtn\">등록</button>\r\n        </form>\r\n      </div>\r\n      <!-- 등록버튼 -->\r\n      <div class=\"resist_pop popup\" v-show=\"resistIs\">\r\n        <div>\r\n          <h3>차량 등록</h3>\r\n          <button @click=\"registClose\">\r\n            <font-awesome-icon icon=\"times-circle\" />\r\n          </button>\r\n          <div class=\"form_window\">\r\n            <form>\r\n              <p>차량번호<span>(필수)</span></p>\r\n              <input\r\n                type=\"text\"\r\n                placeholder=\"이곳에 차량번호를 입력해주세요.\"\r\n                required\r\n                v-model=\"admin.lp\"\r\n              />\r\n              <p>이름<span></span></p>\r\n              <input\r\n                type=\"text\"\r\n                placeholder=\"이곳에 이름을 입력해 주세요.\"\r\n                v-model=\"admin.name\"\r\n              />\r\n              <p>전화번호<span></span></p>\r\n              <input\r\n                type=\"text\"\r\n                placeholder=\"이곳에 전화번호를 입력해 주세요.\"\r\n                v-model=\"admin.phone\"\r\n              />\r\n              <p>구분</p>\r\n              <input\r\n                type=\"text\"\r\n                placeholder=\"이곳에 분류할 내용을 입력해 주세요.\"\r\n                v-model=\"admin.kind1\"\r\n              />\r\n              <p>비고</p>\r\n              <input type=\"text\" v-model=\"admin.contents\" />\r\n              <button @click.prevent=\"registNew\" class=\"writebtn\">등록</button>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- 수정버튼 -->\r\n      <div class=\"modify_pop popup\" v-show=\"modifyIs\">\r\n        <div>\r\n          <h3>차량 정보 수정</h3>\r\n          <button @click=\"modifyClose\">\r\n            <font-awesome-icon icon=\"times-circle\" />\r\n          </button>\r\n          <div class=\"form_window\">\r\n            <form>\r\n              <p>차량번호<span>(필수)</span></p>\r\n              <input type=\"text\" v-model=\"current.lp\" required />\r\n              <p>이름<span></span></p>\r\n              <input type=\"text\" v-model=\"current.name\" />\r\n              <p>전화번호<span></span></p>\r\n              <input type=\"text\" v-model=\"current.phone\" />\r\n              <p>구분<span></span></p>\r\n              <input type=\"text\" v-model=\"current.kind1\" />\r\n              <p>비고<span></span></p>\r\n              <input type=\"text\" v-model=\"current.contents\" />\r\n              <button @click.prevent=\"modifyVehicle\" class=\"modifybtn\">\r\n                수정\r\n              </button>\r\n              <button @click.prevent=\"deleteVehicle\" class=\"deletebtn\">\r\n                삭제\r\n              </button>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"list-items\">\r\n        <ul class=\"list-title\">\r\n          <li>\r\n            <span>\r\n              번호\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span>\r\n              차량번호\r\n              <button @click.prevent=\"item_lp_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.lp_click\"\r\n                /><font-awesome-icon icon=\"caret-up\" v-else />\r\n              </button>\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span>\r\n              이름\r\n              <button @click.prevent=\"item_name_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.name_click\"\r\n                /><font-awesome-icon icon=\"caret-up\" v-else />\r\n              </button>\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span>\r\n              전화번호\r\n              <button @click.prevent=\"item_phone_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.phone_click\"\r\n                /><font-awesome-icon icon=\"caret-up\" v-else />\r\n              </button>\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span>\r\n              구분\r\n              <button @click.prevent=\"item_kind1_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.kind1_click\"\r\n                /><font-awesome-icon icon=\"caret-up\" v-else />\r\n              </button>\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span>\r\n              비고\r\n              <button @click.prevent=\"item_contents_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.contents_click\"\r\n                /><font-awesome-icon icon=\"caret-up\" v-else />\r\n              </button>\r\n            </span>\r\n          </li>\r\n        </ul>\r\n        <!-- 리스트 출력부분 -->\r\n        <ul class=\"list-wrap\">\r\n          <li\r\n            v-for=\"(ad, index) in resistRocord\"\r\n            :key=\"index\"\r\n            @click=\"modifyOpen(ad)\"\r\n          >\r\n            <span> {{ index + 1 }}</span>\r\n            <input type=\"text\" v-model=\"ad.lp\" />\r\n            <input type=\"text\" v-model=\"ad.name\" />\r\n            <input type=\"text\" v-model=\"ad.phone\" />\r\n            <input type=\"text\" v-model=\"ad.kind1\" />\r\n            <input type=\"text\" v-model=\"ad.contents\" />\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport _ from \"underscore\"\r\nexport default {\r\n  data: function() {\r\n    return {\r\n      searchLp: \"\",\r\n      resistRocord: [],\r\n      admin: [\r\n        {\r\n          order: \"\",\r\n          lp: \"\",\r\n          name: \"\",\r\n          phone: \"\",\r\n          kind1: \"\",\r\n          contents: \"\",\r\n          number: \"\",\r\n          check: true\r\n        }\r\n      ],\r\n      current: {\r\n        lp: \"\",\r\n        name: \"\",\r\n        phone: \"\",\r\n        kind1: \"\",\r\n        number: \"\",\r\n        contents: \"\"\r\n      },\r\n      sort_item: {\r\n        lp_click: false,\r\n        name_click: false,\r\n        phone_click: false,\r\n        kind1_click: false,\r\n        contents_click: false\r\n      },\r\n      check: true,\r\n      resistIs: false,\r\n      modifyIs: false\r\n    }\r\n  },\r\n  created() {\r\n    this.$socket.on(\"message_from_server\", data => {\r\n      console.log(\"data.kind : \", data.kind)\r\n      this.black_list(data)\r\n    }),\r\n      this.search1()\r\n  },\r\n  methods: {\r\n    registOpen() {\r\n      this.admin.lp = \"\"\r\n      this.admin.name = \"\"\r\n      this.admin.phone = \"\"\r\n      this.admin.kind1 = \"\"\r\n      this.admin.contents = \"\"\r\n      this.resistIs = true\r\n    },\r\n    registClose() {\r\n      this.resistIs = false\r\n    },\r\n    registNew(e) {\r\n      this.registClose()\r\n      e.preventDefault()\r\n      console.log(\"this.admin : \", this.admin)\r\n      let obj = {}\r\n      obj.lp = this.admin.lp\r\n      obj.name = this.admin.name\r\n      obj.phone = this.admin.phone\r\n      obj.company = this.admin.kind1\r\n      obj.contents = this.admin.contents\r\n      obj.content = this.admin.contents\r\n      obj.number = obj.lp.replace(/[가-힝]/gi, \"\")\r\n      obj.kind = \"create_black_list\"\r\n      console.log(\"obj :\", obj)\r\n      this.sendMessage(obj)\r\n    },\r\n    item_lp_click: function() {\r\n      console.log(\"lp sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.lp_click === false) {\r\n        console.log(\"lp sort1\")\r\n        this.sort_item.lp_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"lp\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"lp sort2\")\r\n        this.sort_item.lp_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"lp\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    item_name_click: function() {\r\n      console.log(\"name sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.name_click === false) {\r\n        console.log(\"name sort1\")\r\n        this.sort_item.name_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"name\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"name sort2\")\r\n        this.sort_item.name_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"name\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    item_phone_click: function() {\r\n      console.log(\"phone sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.phone_click === false) {\r\n        console.log(\"phone sort1\")\r\n        this.sort_item.phone_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"phone\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"phone sort2\")\r\n        this.sort_item.phone_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"phone\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    item_kind1_click: function() {\r\n      console.log(\"kind1 sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.kind1_click === false) {\r\n        console.log(\"kind1 sort1\")\r\n        this.sort_item.kind1_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"kind1\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"kind1 sort2\")\r\n        this.sort_item.kind1_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"kind1\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    item_contents_click: function() {\r\n      console.log(\"contents sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.contents_click === false) {\r\n        console.log(\"contents sort1\")\r\n        this.sort_item.contents_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"contents\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"contents sort2\")\r\n        this.sort_item.contents_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"contents\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    search() {\r\n      console.log(\"검색 버튼을 눌렀습니다.\")\r\n      if (this.searchLp !== \"\") {\r\n        console.log(\"searchlp\")\r\n        for (let i = 0; i < this.resistRocord.length; i++) {\r\n          if (this.searchLp === this.resistRocord[i].lp) {\r\n            let c = this.resistRocord[i]\r\n            this.modifyOpen(c)\r\n            break\r\n          }\r\n        }\r\n      }\r\n    },\r\n    search1() {\r\n      let obj = {}\r\n      obj.kind = \"get_black_list\"\r\n      this.sendMessage(obj)\r\n    },\r\n    black_list(data) {\r\n      console.log(\"get_black_list: \", data)\r\n      if (data.kind === \"get_black_list\") {\r\n        this.resistRocord = []\r\n        for (var i = 0; i < data.docs.length; i++) {\r\n          data.docs[i].kind1 = data.docs[i].company\r\n          var obj = {}\r\n          obj.lp = data.docs[i].lp\r\n          obj.name = \"\"\r\n          if (data.docs[i].name) {\r\n            obj.name = data.docs[i].name\r\n          }\r\n          obj.phone = \"\"\r\n          if (data.docs[i].phone) {\r\n            obj.phone = data.docs[i].phone\r\n          }\r\n          obj.kind1 = \"\"\r\n          if (data.docs[i].kind1) {\r\n            obj.kind1 = data.docs[i].kind1\r\n          }\r\n          obj.contents = \"\"\r\n          if (data.docs[i].contents) {\r\n            obj.contents = data.docs[i].contents\r\n          }\r\n          obj.number = \"\"\r\n          if (data.docs[i].number) {\r\n            obj.number = data.docs[i].number\r\n          }\r\n          this.resistRocord.push(obj)\r\n        }\r\n        console.log(\"this.resistRocord : \", this.resistRocord)\r\n      } else if (\r\n        data.kind === \"create_black_list\" ||\r\n        data.kind === \"update_black_list\" ||\r\n        data.kind === \"delete_black_list\"\r\n      ) {\r\n        this.search1()\r\n      }\r\n    },\r\n    modifyOpen(c) {\r\n      this.modifyIs = true\r\n      this.current.lp = c.lp\r\n      this.current.name = c.name\r\n      this.current.phone = c.phone\r\n      this.current.kind1 = c.kind1\r\n      this.current.contents = c.contents\r\n      console.log(\"c : \", c.lp)\r\n      console.log(\"this : \", this)\r\n    },\r\n    modifyClose() {\r\n      this.modifyIs = false\r\n    },\r\n    searchItems() {\r\n      console.log(\"테스트\")\r\n    },\r\n    sendMessage(c) {\r\n      this.$socket.emit(\"message_from_web_client\", c)\r\n    },\r\n    modifyVehicle() {\r\n      console.log(\"차량정보 수정\")\r\n      this.modifyClose()\r\n      let obj = {}\r\n      obj.lp = this.current.lp\r\n      obj.name = this.current.name\r\n      obj.phone = this.current.phone\r\n      obj.kind1 = this.current.kind1\r\n      obj.company = obj.kind1\r\n      obj.contents = this.current.contents\r\n      obj.number = obj.lp.replace(/[가-힝]/gi, \"\")\r\n      obj.kind = \"update_black_list\"\r\n      this.sendMessage(obj)\r\n    },\r\n    deleteVehicle() {\r\n      console.log(\"차량정보 삭제\")\r\n      this.modifyClose()\r\n      let obj = {}\r\n      obj.lp = this.current.lp\r\n      obj.name = this.current.name\r\n      obj.phone = this.current.phone\r\n      obj.kind1 = this.current.kind1\r\n      obj.company = obj.kind1\r\n      obj.contents = this.current.contents\r\n      obj.number = obj.lp.replace(/[가-힝]/gi, \"\")\r\n      obj.kind = \"delete_black_list\"\r\n      this.sendMessage(obj)\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n.body_wrap {\r\n  height: auto;\r\n}\r\n\r\n/* #conents-wrap {\r\n  padding-bottom: 400px;\r\n} */\r\n\r\n/* 테이블데이터 */\r\n/* .content_wrap {\r\n  height: 100%;\r\n} */\r\n.content_wrap .menu_title {\r\n  display: flex;\r\n  margin-bottom: 15px;\r\n  height: 41px;\r\n  justify-content: flex-end;\r\n}\r\n.content_wrap .menu_title span.small_title {\r\n  background-color: #fafafa;\r\n  padding: 5px;\r\n  border: 1px solid #c6c6c6;\r\n  background-color: #fafafa;\r\n  padding: 0px 7px;\r\n  height: 100%;\r\n  display: block;\r\n  text-align: center;\r\n  line-height: 41px;\r\n}\r\n.content_wrap .menu_title span.small_title:nth-of-type(3) {\r\n  margin-left: 10px;\r\n}\r\n.content_wrap .menu_title form {\r\n  width: 100%;\r\n  display: -webkit-box;\r\n  display: -ms-flexbox;\r\n  display: flex;\r\n  -webkit-box-align: center;\r\n  -ms-flex-align: center;\r\n  align-items: center;\r\n}\r\n.content_wrap .menu_title form input {\r\n  border: 1px solid #c6c6c6;\r\n  margin-left: -1px;\r\n  padding: 10px 10px;\r\n  width: 18%;\r\n  height: 41px;\r\n}\r\n.content_wrap .menu_title span.small_title:nth-of-type(2) {\r\n  margin-left: 10px;\r\n}\r\n\r\n.content_wrap .menu_title form input[type=\"datetime-local\"] {\r\n  font-size: 14px;\r\n  padding: 9px 5px;\r\n}\r\n.content_wrap .menu_title form input::placeholder {\r\n  color: #bababa;\r\n  background: url(../assets/zoom_table.png) no-repeat 100% center;\r\n  padding: 0 20px 0 0px;\r\n}\r\n.content_wrap .menu_title button.seach_confirm {\r\n  background: #29a95c !important;\r\n  margin-left: 10px;\r\n}\r\n.content_wrap .menu_title button.registerbtn {\r\n  margin-left: auto;\r\n}\r\n.content_wrap .menu_title button {\r\n  color: white;\r\n  margin-right: 5px;\r\n  background: #6a6cff !important;\r\n  width: 79px;\r\n  border-radius: 5px;\r\n  color: white;\r\n  height: 100%;\r\n  transition: all 0.1s ease;\r\n}\r\n.content_wrap .menu_title button.modifybtn {\r\n  background: #404040 !important;\r\n}\r\n.content_wrap .menu_title button.deletebtn {\r\n  background: #e74c3c !important;\r\n}\r\n.content_wrap .menu_title button:hover {\r\n  box-shadow: rgba(0, 0, 0, 0.2) 2px 2px 3px 0px inset;\r\n}\r\n.content_wrap .menu_title button:active {\r\n  box-shadow: 2px 1px 2px 2px rgba(0, 0, 0, 0.1) inset;\r\n}\r\n\r\n.list-items .list-wrap li {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  text-align: center;\r\n  border-bottom: 1px solid #c9c9c9;\r\n  align-items: center;\r\n  /* border: 1px solid #ffb300; */\r\n  transition: all 0.1s ease;\r\n  cursor: pointer;\r\n}\r\n.list-items .list-wrap li span {\r\n  width: 50%;\r\n}\r\n.content_wrap .list-items .list-wrap li input {\r\n  cursor: pointer;\r\n}\r\n.content_wrap .list-items .list-wrap li:hover {\r\n  border: 2px solid #ffb300;\r\n  /* box-shadow: 1px 1px 2px 2px rgba(0, 0, 0, 0.1) inset; */\r\n}\r\n.content_wrap .list-items .list-wrap li:active {\r\n  box-shadow: 1px 1px 2px 2px rgba(0, 0, 0, 0.1) inset;\r\n}\r\n.list-items .list-title {\r\n  background: #6a6cff;\r\n  position: absolute;\r\n  top: 1px;\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: space-around;\r\n  padding: 7px 0;\r\n  color: #fff;\r\n}\r\n.list-items .list-title > li {\r\n  width: 100%;\r\n  text-align: center;\r\n}\r\n.list-items .list-title > li:first-child {\r\n  width: 50%;\r\n}\r\n.list-items .list-title span {\r\n  width: 100%;\r\n  text-align: center;\r\n  color: #fff;\r\n}\r\n.list-items .list-title span