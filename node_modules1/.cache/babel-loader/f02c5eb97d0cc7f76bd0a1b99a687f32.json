  <ul class=\"list-title\">\r\n          <li>\r\n            <span>번호</span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >차량번호<button @click.prevent=\"item_lp_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.lp\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >입차시각<button @click.prevent=\"in_time_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.in_time\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >방문자 확인시각<button @click.prevent=\"confirmTime_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.confirmTime\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >방문자 할인시간<button\r\n                @click.prevent=\"discounted_duration_click\"\r\n              >\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.discounted_duration\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >입주사 ID<button @click.prevent=\"id_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.id\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >입주사 부담금액<button @click.prevent=\"charge_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.charge\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span>차량사진</span>\r\n          </li>\r\n        </ul>\r\n        <ul class=\"list-wrap\">\r\n          <li v-for=\"(c, index) in residents\" :key=\"index\">\r\n            <span> {{ index + 1 }}</span>\r\n            <input type=\"text\" v-model=\"c.lp\" />\r\n            <span>{{ c.time }}</span>\r\n            <span>{{ c.confirmTime }}</span>\r\n            <input type=\"text\" v-model=\"c.discounted_duration\" />\r\n            <input type=\"text\" v-model=\"c.id\" />\r\n            <input type=\"text\" v-model=\"c.charge\" />\r\n            <span\r\n              ><button @click.prevent=\"openImg(c)\">\r\n                <font-awesome-icon icon=\"camera-retro\" /></button\r\n            ></span>\r\n\r\n            <div class=\"imageshow\" v-show=\"c.imageShow\">\r\n              <div>\r\n                <h3>차량 이미지</h3>\r\n                <button @click.prevent=\"closeImg(c)\">\r\n                  <font-awesome-icon icon=\"times-circle\" />\r\n                </button>\r\n                <img :src=\"c.image_name\" alt=\"\" />\r\n                <div>\r\n                  <span>차량번호 : {{ c.lp }}</span>\r\n                  <span>입차시각 : {{ c.time }}</span>\r\n                  <span>할인시간 : {{ c.discounted_duration }}</span>\r\n                  <span>입주사 부담금액 : {{ c.charge }}원</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport _ from \"underscore\"\r\nexport default {\r\n  data() {\r\n    return {\r\n      start_time: new Date(),\r\n      end_time: new Date(),\r\n      searchNumber: \"\",\r\n      btn: \"\",\r\n      residents: [\r\n        /*{\r\n\t\t\t\torder: \"0\",\r\n\t\t\t\tlp: \"29가1234\",\r\n\t\t\t\ttime:  format_time1(new Date(1586297735243)),\r\n\t\t\t\tconfirmTime: format_time1(new Date(1586297735243)),\r\n\t\t\t\tdiscounted_duration : \"60\",\r\n\t\t\t\tcharge: \"1000\",\r\n\t\t\t\timageName: require(\"../assets/car_test1.jpg\"),\r\n\t\t\t\timageShow: false\r\n\t\t\t\t}*/\r\n      ],\r\n      sort_item: {\r\n        lp: false,\r\n        in_time: false,\r\n        out_time: false,\r\n        confirmTime: false,\r\n        discounted_duration: false,\r\n        id: false,\r\n        charge: false\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    item_lp_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.lp === false) {\r\n        this.sort_item.lp = true\r\n        temp10 = _.sortBy(this.residents, \"lp\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.lp = false\r\n        temp10 = _.sortBy(this.residents, \"lp\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    in_time_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.in_time === false) {\r\n        this.sort_item.in_time = true\r\n        temp10 = _.sortBy(this.residents, \"in_time\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.in_time = false\r\n        temp10 = _.sortBy(this.residents, \"in_time\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    out_time_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.out_time === false) {\r\n        this.sort_item.out_time = true\r\n        temp10 = _.sortBy(this.residents, \"out_time\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.out_time = false\r\n        temp10 = _.sortBy(this.residents, \"out_time\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    confirmTime_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.confirmTime === false) {\r\n        this.sort_item.confirmTime = true\r\n        temp10 = _.sortBy(this.residents, \"confirmTime\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.confirmTime = false\r\n        temp10 = _.sortBy(this.residents, \"confirmTime\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    discounted_duration_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.discounted_duration === false) {\r\n        this.sort_item.discounted_duration = true\r\n        temp10 = _.sortBy(this.residents, \"discounted_duration\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.discounted_duration = false\r\n        temp10 = _.sortBy(this.residents, \"discounted_duration\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    id_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.id === false) {\r\n        this.sort_item.id = true\r\n        temp10 = _.sortBy(this.residents, \"id\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.id = false\r\n        temp10 = _.sortBy(this.residents, \"id\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    charge_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.charge === false) {\r\n        this.sort_item.charge = true\r\n        temp10 = _.sortBy(this.residents, \"charge\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.charge = false\r\n        temp10 = _.sortBy(this.residents, \"charge\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    socket_process(data) {\r\n      console.log(\"socket_process===>\", data)\r\n      if (\r\n        data.status === \"ok\" &&\r\n        data.kind === \"get_resident_discounted_list_admin\"\r\n      ) {\r\n        console.log(\"socket_process for ===> : \", data)\r\n        for (var i = 0; i < data.docs.length; i++) {\r\n          var obj1 = data.docs[i]\r\n          for (var j = 0; j < obj1.discounted_list.length; j++) {\r\n            for (var k = j + 1; k < obj1.discounted_list.length; k++) {\r\n              if (obj1.discounted_list[j].id === obj1.discounted_list[k].id) {\r\n                obj1.discounted_list[k].splice(k, 1)\r\n              }\r\n            }\r\n            if (this.searchNumber === \"\") {\r\n              let obj2 = {}\r\n              obj2.image_name = //       \"http://test.sunil-ezp.com/images/\" + obj1.folder_name + \"/\" + obj1.fname\r\n                \"http://localhost:3002/images/\" +\r\n                obj1.folder_name +\r\n                \"/\" +\r\n                obj1.fname\r\n              console.log(\" obj2.image_name : \", obj2.image_name)\r\n              obj2.lp = obj1.lp\r\n              obj2.time = format_time1(new Date(obj1.loop_event_time))\r\n              obj2.confirmTime = format_time1(\r\n                new Date(obj1.discounted_list[j].discounted_time)\r\n              )\r\n              obj2.discounted_duration =\r\n                Math.ceil(\r\n                  parseInt(obj1.discounted_list[j].discounted_duration),\r\n                  10\r\n                ) /\r\n                (60 * 1000)\r\n              obj2.id = obj1.discounted_list[j].id\r\n              obj2.charge = \"\"\r\n              obj2.imageShow = false\r\n              this.residents.push(obj2)\r\n              break\r\n            } else {\r\n              if (obj1.lp.search(this.searchNumber) >= 0) {\r\n                let obj2 = {}\r\n                obj2.image_name =\r\n                  \"http://localhost:3002/images/\" +\r\n                  obj1.folder_name +\r\n                  \"/\" +\r\n                  obj1.fname\r\n                obj2.lp = obj1.lp\r\n                obj2.time = format_time1(new Date(obj1.loop_event_time))\r\n                obj2.confirmTime = format_time1(\r\n                  new Date(obj1.discounted_list[j].discounted_time)\r\n                )\r\n                obj2.discounted_duration =\r\n                  Math.ceil(\r\n                    parseInt(obj1.discounted_list[j].discounted_duration),\r\n                    10\r\n                  ) /\r\n                  (60 * 1000)\r\n                obj2.id = obj1.discounted_list[j].id\r\n                obj2.charge = \"\"\r\n                obj2.imageShow = false\r\n                this.residents.push(obj2)\r\n                break\r\n              }\r\n            }\r\n          }\r\n        }\r\n        //console.log(this.vehicleInformation.length);\r\n        console.log(this.residents.length)\r\n        console.log(this.residents)\r\n      }\r\n    },\r\n    listAdd() {\r\n      // this.dataList = this.inputList\r\n      this.dataList.unshift({\r\n        getTimer: this.listTimer,\r\n        getPc: this.listPc,\r\n        getPlace: this.listPlace,\r\n        getWay: this.listWay,\r\n        getCarNumer: this.listCarNumer,\r\n        getType: this.listType,\r\n        getLocation: this.listLocation,\r\n        getMemo: this.listMemo\r\n      })\r\n      this.listTimer = \"\"\r\n      this.listPc = \"\"\r\n      this.listPlace = \"\"\r\n      this.listWay = \"\"\r\n      this.listCarNumer = \"\"\r\n      this.listType = \"\"\r\n      this.listLocation = \"\"\r\n      this.listMemo = \"\"\r\n    },\r\n    date_set() {\r\n      let d = new Date()\r\n      //let d1 = d - (24*60*60*1000);\r\n      //console.log(\"d : \", d, \" d1 : \", d1);\r\n      var str_start =\r\n        d.getFullYear() +\r\n        \"-\" +\r\n        itoStr(d.getMonth() + 1) +\r\n        \"-\" +\r\n        itoStr(d.getDate()) +\r\n        \"T00:00\"\r\n      var str_end =\r\n        d.getFullYear() +\r\n        \"-\" +\r\n        itoStr(d.getMonth() + 1) +\r\n        \"-\" +\r\n        itoStr(d.getDate()) +\r\n        \"T23:59\"\r\n      this.start_time = str_start\r\n      this.end_time = str_end\r\n    },\r\n    sendMessage(c) {\r\n      this.$socket.emit(\"message_from_web_client\", c)\r\n    },\r\n    search() {\r\n      this.residents = []\r\n      var obj = {}\r\n      obj.kind = \"get_resident_discounted_list_admin\"\r\n      obj.lp = this.searchNumber\r\n      obj.start_time = get_start_time(this.start_time)\r\n      obj.end_time = get_end_time(this.end_time)\r\n      obj.id = window.localStorage.getItem(\"admin_id\")\r\n      obj.pw = window.localStorage.getItem(\"admin_pw\")\r\n      console.log(\"lp_search() : \", obj)\r\n      this.sendMessage(obj)\r\n    },\r\n    openImg(c) {\r\n      return (c.imageShow = true)\r\n      //console.log(\" openImg c : \", c);\r\n    },\r\n    closeImg(c) {\r\n      return (c.imageShow = false)\r\n      // console.log(\" closeImg c : \", c);\r\n    }\r\n  },\r\n  created() {\r\n    this.$socket.on(\"message_from_server\", data => {\r\n      console.log(\"resident_discount_data.kind : \", data)\r\n      this.socket_process(data)\r\n    })\r\n    this.date_set()\r\n    window.addEventListener(\"scroll\", this.handleScroll)\r\n  }\r\n}\r\n\r\nfunction format_time1(dat) {\r\n  var str = \"\"\r\n  str += dat.getFullYear()\r\n  str += \".\" + num_to_str(dat.getMonth() + 1)\r\n  str += \".\" + num_to_str(dat.getDate())\r\n  str += \". \" + num_to_str(dat.getHours())\r\n  str += \":\" + num_to_str(dat.getMinutes())\r\n  str += \":\" + num_to_str(dat.getSeconds())\r\n  return str\r\n}\r\n\r\nfunction num_to_str($num) {\r\n  $num < 10 ? ($num = \"0\" + $num) : $num\r\n  return $num.toString()\r\n}\r\n/*\r\nfunction get_datetime(str) {\r\n\tvar res = str.split(/[-T.:\\s]+/);\r\n\tvar d = new Date(parseInt(res[0]), parseInt(res[1])-1, parseInt(res[2]), parseInt(res[3]), parseInt(res[4]), 0, 0);\r\n\treturn d.getTime();\r\n} */\r\n\r\nfunction get_start_time(str) {\r\n  var res = str.split(/[-T.:\\s]+/)\r\n  var d = new Date(\r\n    parseInt(res[0]),\r\n    parseInt(res[1]) - 1,\r\n    parseInt(res[2]),\r\n    parseInt(res[3]),\r\n    parseInt(res[4]),\r\n    0,\r\n    0\r\n  )\r\n  return d.getTime()\r\n}\r\n\r\nfunction get_end_time(str) {\r\n  var res = str.split(/[-T.:\\s]+/)\r\n  var d = new Date(\r\n    parseInt(res[0]),\r\n    parseInt(res[1]) - 1,\r\n    parseInt(res[2]),\r\n    parseInt(res[3]),\r\n    parseInt(res[4]),\r\n    59,\r\n    999\r\n  )\r\n  return d.getTime()\r\n}\r\n\r\nfunction itoStr($num) {\r\n  $num < 10 ? ($num = \"0\" + $num) : $num\r\n  return $num.toString()\r\n}\r\n</script>\r\n<style scoped>\r\n.body_wrap {\r\n  height: auto;\r\n}\r\n\r\n/* 테이블데이터 */\r\n.content_wrap {\r\n  height: 100%;\r\n}\r\n.content_wrap .menu_title {\r\n  display: flex;\r\n  margin-bottom: 15px;\r\n  height: 41px;\r\n}\r\n.content_wrap .menu_title form {\r\n  width: 100%;\r\n  display: flex;\r\n\r\n  -webkit-box-align: center;\r\n  -ms-flex-align: center;\r\n  align-items: center;\r\n}\r\n.content_wrap .menu_title form input {\r\n  border: 1px solid #c6c6c6;\r\n  margin-left: -1px;\r\n  padding: 10px 5px;\r\n  width: 18%;\r\n  height: 41px;\r\n}\r\n.content_wrap .menu_title form input[type=\"datetime-local\"] {\r\n  font-size: 14px;\r\n  padding: 9px 5px;\r\n}\r\n.content_wrap .menu_title form input::placeholder {\r\n  color: #bababa;\r\n  background: url(../assets/zoom_table.png) no-repeat 100% center;\r\n  padding: 0 20px 0 0px;\r\n}\r\n.content_wrap .menu_title form span:nth-child(3) {\r\n  margin: 0 5px;\r\n}\r\n\r\n.content_wrap .menu_title .seach_confirm {\r\n  color: white;\r\n  background: #29a95c !important;\r\n  width: 79px;\r\n  border-radius: 5px;\r\n  color: white;\r\n  height: 100%;\r\n  margin-left: 10px;\r\n  transition: all 0.1s ease;\r\n}\r\n.content_wrap .menu_title .seach_confirm:hover {\r\n  box-shadow: rgba(0, 0, 0, 0.2) 2px 2px 3px 0px inset;\r\n}\r\n.content_wrap .menu_title .seach_confirm:active {\r\n  box-shadow: 2px 1px 2px 2px rgba(0, 0, 0, 0.1) inset;\r\n}\r\n.content_wrap .menu_title span.small_title {\r\n  background-color: #fafafa;\r\n  padding: 5px;\r\n  border: 1px solid #c6c6c6;\r\n  background-color: #fafafa;\r\n  padding: 0px 7px;\r\n  height: 100%;\r\n  display: block;\r\n  text-align: center;\r\n  line-height: 41px;\r\n}\r\n.content_wrap .menu_title span.small_title:nth-of-type(3) {\r\n  margin-left: 10px;\r\n}\r\n.content_wrap .menu_title form {\r\n  width: 100%;\r\n  display: -webkit-box;\r\n  display: -ms-flexbox;\r\n  display: flex;\r\n  -webkit-box-align: center;\r\n  -ms-flex-align: center;\r\n  align-items: center;\r\n}\r\n\r\n.list-items .list-title {\r\n  background: #6a6cff;\r\n  position: absolute;\r\n  top: 1px;\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: space-around;\r\n  padding: 7px 0;\r\n  color: #fff;\r\n}\r\n.list-items .list-title > li {\r\n  width: 100%;\r\n}\r\n.list-items .list-title > li:first-child {\r\n  width: 50%;\r\n}\r\n.list-items .list-wrap li {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  text-align: center;\r\n  border-bottom: 1px solid #c9c9c9;\r\n  align-items: center;\r\n  /* border: 1px solid #ffb300; */\r\n  transition: all 0.1s ease;\r\n}\r\n.list-items .list-title span {\r\n  width: 100%;\r\n  text-align: center;\r\n  color: #fff;\r\n}\r\n.list-items .list-title span .svg-inline--fa {\r\n  margin: 0 0 0 5px;\r\n}\r\n\r\n.list-items ul.list-wrap li span:first-child {\r\n  width: 50%;\r\n}\r\n\r\n.content_wrap .list-items .list-wrap li:hover {\r\n  border: 2px solid #ffb300;\r\n  /* box-shadow: 1px 1px 2px 2px rgba(0, 0, 0, 0.1) inset; */\r\n}\r\n.list-items ul li input {\r\n  width: 100%;\r\n  outline: none;\r\n  height: 34px;\r\n  text-align: center;\r\n}\r\n.list-items ul li span {\r\n  display: inline-block;\r\n  width: 100%;\r\n}\r\n.list-items ul li > button {\r\n  width: 100%;\r\n  font-size: 23px;\r\n}\r\n.list-items ul li .imageshow {\r\n  position: fixed;\r\n  width: 100%;\r\n  height: 100%;\r\n  top: 50%;\r\n  z-index: 10000;\r\n  transform: translate(-50%, -50%);\r\n  left: 50%;\r\n  background: rgba(0, 0, 0, 0.4);\r\n}\r\n.list-items ul li .imageshow > div {\r\n  background: #333333;\r\n  padding: 5px 10px 10px;\r\n  width: 36%;\r\n  margin: 14% auto;\r\n  border-radius: 5px;\r\n}\r\n.list-items ul li .imageshow h3 {\r\n  color: #fff;\r\n  float: left;\r\n  line-height: 35px;\r\n}\r\n.list-items ul li .imageshow button {\r\n  font-size: 35px;\r\n  color: #29a95c;\r\n  display: block;\r\n  margin-left: auto;\r\n}\r\n.list-items ul li .imageshow img {\r\n  width: 100%;\r\n  margin-bottom: -5px;\r\n}\r\n.list-items ul li .imageshow div div {\r\n  color: #fff;\r\n  text-align: left;\r\n  min-width: 440px;\r\n  background: #6a6cff;\r\n  font-size: 15px;\r\n  padding: 10px 5px;\r\n}\r\n.list-items ul li .imageshow div div span {\r\n  width: 50%;\r\n  padding: 7px 0;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}