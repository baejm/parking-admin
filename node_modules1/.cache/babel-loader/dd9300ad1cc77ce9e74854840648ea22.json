LoginError(state) {\n      state.loginSuccess = false\n      state.loginError = true\n    },\n    logout(state) {\n      state.loginSuccess = false\n      state.loginError = false\n      state.userGetDb = \"\"\n      router.push({ name: \"login\" })\n    },\n    freshOn(state) {\n      state.fresh = true\n    },\n    freshOff(state) {\n      state.fresh = false\n    }\n  },\n  actions: {\n    login({ state, commit }, payload) {\n      let selectedUser = \"\"\n      state.userDb.forEach(user => {\n        if (user.id === payload.id) selectedUser = user\n      })\n      if (selectedUser === \"\" || selectedUser.password !== payload.password) {\n        commit(\"isLoginError\")\n      } else {\n        commit(\"isLoginSuccess\", selectedUser)\n        router.push({ name: \"live_vehicle\" })\n        console.log(\"login 성공\")\n      }\n    },\n    logout({ commit }) {\n      commit(\"logout\")\n      router.push({ name: \"login\" })\n    },\n    freshOn({ commit }) {\n      commit(\"freshOn\")\n    },\n    freshOff({ commit }) {\n      commit(\"freshOff\")\n    }\n  },\n  modules: {}\n})\n"]}]}