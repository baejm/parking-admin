w = payload.admin_pw\n      state.userGetDb.admin_status = \"ok\"\n    },\n    isLoginError(state) {\n      // localStorage.admin_id = \"\"\n      // localStorage.admin_pw = \"\"\n      // localStorage.admin_status = \"\"\n      state.loginSuccess = false\n      state.loginError = true\n    },\n    logout(state) {\n      console.log(\"logout\")\n      localStorage.admin_id = \"\"\n      localStorage.admin_pw = \"\"\n      localStorage.admin_status = \"\"\n      state.loginSuccess = false\n      state.loginError = false\n      state.userGetDb = {}\n     // router.push({ name: \"login\" })\n    },\n    freshOn(state) {\n      state.fresh = true\n    },\n    freshOff(state) {\n      state.fresh = false\n    }\n  },\n  actions: {\n    login({ commit }, payload) {\n      console.log(\"payload : \", payload)\n      commit(\"isLoginSuccess\", payload)\n      router.push({ name: \"live_vehicle\" })\n      console.log(\"login 성공\")\n    },\n    logout({ commit }) {\n      commit(\"logout\")\n      router.push({ name: \"login\" })\n    },\n    freshOn({ commit }) {\n      commit(\"freshOn\")\n    },\n    freshOff({ commit }) {\n      commit(\"freshOff\")\n    }\n  },\n  modules: {}\n})\n"]}]}