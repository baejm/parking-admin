     <th>\r\n                전화번호\r\n                <button @click.prevent=\"item_phone_click\">\r\n                  <font-awesome-icon\r\n                    icon=\"caret-down\"\r\n                    v-if=\"sort_item.phone_click\"\r\n                  /><font-awesome-icon icon=\"caret-up\" v-else />\r\n                </button>\r\n              </th>\r\n\r\n              <th>\r\n                구분\r\n                <button @click.prevent=\"item_kind1_click\">\r\n                  <font-awesome-icon\r\n                    icon=\"caret-down\"\r\n                    v-if=\"sort_item.kind1_click\"\r\n                  /><font-awesome-icon icon=\"caret-up\" v-else />\r\n                </button>\r\n              </th>\r\n\r\n              <th>\r\n                비고\r\n                <button @click.prevent=\"item_contents_click\">\r\n                  <font-awesome-icon\r\n                    icon=\"caret-down\"\r\n                    v-if=\"sort_item.contents_click\"\r\n                  /><font-awesome-icon icon=\"caret-up\" v-else />\r\n                </button>\r\n              </th>\r\n            </tr>\r\n          </thead>\r\n          <!-- 리스트 출력부분 -->\r\n          <tbody class=\"list-wrap\">\r\n            <tr\r\n              v-for=\"(ad, index) in resistRocord\"\r\n              :key=\"index\"\r\n              @click=\"modifyOpen(ad)\"\r\n            >\r\n              <td>{{ index + 1 }}</td>\r\n              <td>{{ ad.lp }}</td>\r\n              <td>{{ ad.name }}</td>\r\n              <td>{{ ad.phone }}</td>\r\n              <td>{{ ad.kind1 }}</td>\r\n              <td>{{ ad.contents }}</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport _ from \"underscore\"\r\nexport default {\r\n  data: function() {\r\n    return {\r\n      searchLp: \"\",\r\n      resistRocord: [],\r\n      admin: [\r\n        {\r\n          order: \"\",\r\n          lp: \"\",\r\n     