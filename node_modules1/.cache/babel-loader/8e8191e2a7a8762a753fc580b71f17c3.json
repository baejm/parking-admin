/form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- 수정버튼 -->\r\n      <div class=\"modify_pop popup\" v-show=\"modifyIs\">\r\n        <div>\r\n          <h3>할인권 판매 리스트</h3>\r\n          <button @click=\"modifyClose\">\r\n            <font-awesome-icon icon=\"times-circle\" />\r\n          </button>\r\n          <div class=\"form_window\">\r\n            <form>\r\n              <p>코드<span></span></p>\r\n              <input type=\"text\" v-model=\"current.code\" required />\r\n              <p>할인권종<span></span></p>\r\n              <input type=\"text\" v-model=\"current.discount_coupon_kind\" />\r\n              <p>가격(원)<span></span></p>\r\n              <input type=\"text\" v-model=\"current.price\" />             \r\n              <p>비고<span></span></p>\r\n              <input type=\"text\" v-model=\"current.contents\" />\r\n              <button @click.prevent=\"modifyVehicle\" class=\"modifybtn\">\r\n                수정\r\n              </button>\r\n              <button @click.prevent=\"deleteVehicle\" class=\"deletebtn\">\r\n                삭제\r\n              </button>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"list-items\">\r\n        <!-- 리스트 상단 메뉴 -->\r\n        <ul class=\"list-title\">\r\n          <li>\r\n            <span>\r\n              번호\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span>\r\n              판매자ID\r\n              <button @click.prevent=\"item_ID_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.ID_click\"\r\n                /><font-awesome-icon icon=\"caret-up\" v-else />\r\n              </button>\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span>\r\n              할인권종\r\n              <button @click.prevent=\"item_discount_coupon_kind_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.discount_coupon_kind_click\"\r\n                /><font-awesome-icon icon=\"caret-up\" v-else />\r\n              </button>\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span>\r\n              가격\r\n              <button @click.prevent=\"item_price_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.price_click\"\r\n                /><font-awesome-icon icon=\"caret-up\" v-else />\r\n              </button>\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span>\r\n              비고\r\n              <button @click.prevent=\"item_contents_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.contents_click\"\r\n                /><font-awesome-icon icon=\"caret-up\" v-else />\r\n              </button>\r\n            </span>\r\n          </li>\r\n        </ul>\r\n        <!-- 리스트 출력부분 -->\r\n        <ul class=\"list-wrap\">\r\n          <li\r\n            v-for=\"(ad, index) in resistRocord\"\r\n            :key=\"index\"\r\n            @click=\"modifyOpen(ad)\"\r\n          >\r\n            <span> {{ index + 1 }}</span>\r\n            <span class=\"id\">\r\n              <input type=\"text\" v-model=\"ad.code\"\r\n            /></span>\r\n            <span class=\"residentPW\"\r\n              ><input type=\"text\" v-model=\"ad.discount_coupon_kind\"\r\n            /></span>    \r\n            <span class=\"residentPW\"\r\n              ><input type=\"text\" v-model=\"ad.price\"\r\n            /></span>         \r\n            <span class=\"contents\"\r\n              ><input type=\"text\" v-model=\"ad.contents\"\r\n            /></span>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport _ from \"underscore\"\r\nimport XLSX from \"xlsx\"\r\nimport config from \"../config.js\"\r\nexport default {\r\n  data: function() {\r\n    return {\r\n      resistRocord: [],\r\n      print_resistRocord: [],\r\n      manager_class: \"admin\",\r\n      admin: [\r\n        {\r\n          order: \"\",\r\n          code: \"\",\r\n          discount_coupon_kind: \"\",\r\n          price: \"\",\r\n          contents: \"\",\r\n          check: true\r\n        }\r\n      ],\r\n      current: {\r\n        order: \"\",\r\n          code: \"\",\r\n          discount_coupon_kind: \"\",\r\n          price: \"\",\r\n          contents: \"\"\r\n      },\r\n      sort_item: {\r\n        code_click: false,\r\n        discount_coupon_kind_click: false,\r\n        price_click: false,\r\n        contents_click: false\r\n      },\r\n      id: \"\",\r\n      check: true,\r\n      resistIs: false,\r\n      modifyIs: false\r\n    }\r\n  },\r\n  created() {\r\n    this.$socket.on(\"message_from_server\", data => {\r\n      console.log(\"data.kind : \", data.kind)\r\n      this.discount_coupon_policy(data)\r\n    }),\r\n    this.search1()\r\n  },\r\n  methods: {\r\n    registOpen() {\r\n      this.admin.code = \"\"\r\n      this.admin.discount_coupon_kind = \"\"\r\n      this.admin.price = \"\"\r\n      this.admin.contents = \"\"\r\n      this.resistIs = true\r\n    },\r\n    registClose() {\r\n      this.resistIs = false\r\n    },\r\n    item_code_click: function() {\r\n      console.log(\"code sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.code_click === false) {\r\n        console.log(\"code sort1\")\r\n        this.sort_item.code_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"code\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"code sort2\")\r\n        this.sort_item.id_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"code\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    item_discount_coupon_kind_click: function() {\r\n      console.log(\"discount_coupon_kind sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.discount_coupon_kind_click === false) {\r\n        console.log(\"discount_coupon_kind sort1\")\r\n        this.sort_item.discount_coupon_kind_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"discount_coupon_kind\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"discount_coupon_kind sort2\")\r\n        this.sort_item.discount_coupon_kind_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"discount_coupon_kind\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    item_price_click: function() {\r\n      console.log(\"price sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.price_click === false) {\r\n        console.log(\"price sort1\")\r\n        this.sort_item.price_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"price\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"price sort2\")\r\n        this.sort_item.price_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"price\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },   \r\n    item_contents_click: function() {\r\n      console.log(\"contents sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.contents_click === false) {\r\n        console.log(\"contents sort1\")\r\n        this.sort_item.contents_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"contents\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"contents sort2\")\r\n        this.sort_item.contents_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"contents\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    registNew(e) {\r\n      this.registClose()\r\n      e.preventDefault()\r\n      console.log(\"this.admin : \", this.admin)\r\n      let obj = {}\r\n      obj.code = this.admin.code\r\n      obj.discount_coupon_kind = this.admin.discount_coupon_kind\r\n      obj.price = this.admin.price\r\n      obj.contents = this.admin.contents\r\n      obj.id = this.id\r\n      obj.kind = \"create_discount_coupon_policy\"\r\n      obj.reg_time = Date.now()\r\n      console.log(\"obj :\", obj)\r\n      this.sendMessage(obj)\r\n    },\r\n    search() {\r\n      console.log(\"검색 버튼을 눌렀습니다.\")\r\n      this.search1()\r\n    },\r\n    search1() {\r\n      this.id = localStorage.id\r\n      let obj = {}\r\n      obj.kind = \"get_discount_coupon_policy\"\r\n      this.sendMessage(obj)\r\n    },\r\n    discount_coupon_policy(data) {\r\n      console.log(\"data.kind: \", data.kind, data)\r\n      if (data.kind === \"get_discount_coupon_policy\") {\r\n        this.resistRocord = []\r\n        this.print_resistRocord = []\r\n        for (var i = 0; i < data.docs.length; i++) {\r\n          var obj = {}\r\n          let print = {}\r\n          obj.code = data.docs[i].code\r\n          print.코드= data.docs[i].code\r\n          obj.discount_coupon_kind = \"\"\r\n          if (data.docs[i].discount_coupon_kind) {\r\n            obj.discount_coupon_kind= data.docs[i].discount_coupon_kind\r\n            print.할인권종 = data.docs[i].discount_coupon_kind\r\n          }\r\n          obj.price = 0\r\n          if (data.docs[i].price) {\r\n            obj.price = data.docs[i].price\r\n            print.가격= data.docs[i].price\r\n          } else {\r\n            obj.price = 0\r\n            print.가격 = 0\r\n          } \r\n          obj.reg_time = \"\"\r\n          if (data.docs[i].reg_time) {\r\n            obj.reg_time = data.docs[i].reg_time\r\n            print.등록일시 = data.docs[i].reg_time\r\n          } else {\r\n            obj.reg_time = Date.now()\r\n            print.클래스 = Date.now()\r\n          }\r\n          obj.contents = \"\"\r\n          if (data.docs[i].contents) {\r\n            obj.contents = data.docs[i].contents\r\n            print.비고 = data.docs[i].contents\r\n          } else {\r\n            obj.contents = \"\"\r\n            print.비고 = \"\"\r\n          }\r\n          this.resistRocord.push(obj)\r\n          this.print_resistRocord.push(print)\r\n        }\r\n        console.log(\"this.resistRocord : \", this.resistRocord)\r\n      } else if (\r\n        data.kind === \"create_discount_coupon_policy\" ||\r\n        data.kind === \"update_discount_coupon_policy\" ||\r\n        data.kind === \"delete_discount_coupon_policy\"\r\n      ) {\r\n        this.search1()\r\n      }\r\n    },\r\n    modifyOpen(c) {\r\n      this.modifyIs = true\r\n      this.current.code = c.code\r\n      this.current.discount_coupon_kind = c.discount_coupon_kind\r\n      this.current.price = c.price\r\n      this.current.reg_time = c.reg_time     \r\n      this.current.contents = c.contents\r\n      console.log(\"c : \", c.code)\r\n      console.log(\"this : \", this)\r\n    },\r\n    modifyClose() {\r\n      this.modifyIs = false\r\n    },\r\n    searchItems() {\r\n      console.log(\"테스트\")\r\n    },\r\n    sendMessage(c) {\r\n      this.$socket.emit(\"message_from_web_client\", c)\r\n    },\r\n    modifyVehicle() {\r\n      console.log(\"차량정보 수정\")\r\n      this.modifyClose()\r\n      let obj = {}\r\n      obj.code = this.current.code\r\n      obj.discount_coupon_kind = this.current.discount_coupon_kind\r\n      obj.price = this.current.price\r\n      obj.reg_time = this.current.reg_time\r\n      obj.contents = this.current.contents\r\n      obj.id = this.id\r\n      obj.kind = \"update_discount_coupon_policy\"\r\n      this.sendMessage(obj)\r\n    },\r\n    deleteVehicle() {\r\n      console.log(\"차량정보 삭제\")\r\n      this.modifyClose()\r\n      let obj = {}\r\n      obj.code = this.current.code\r\n      obj.discount_coupon_kind = this.current.discount_coupon_kind\r\n      obj.price = this.current.price\r\n      obj.reg_time = this.current.reg_time\r\n      obj.content = this.current.contents\r\n      obj.id = this.id\r\n      obj.kind = \"delete_discount_coupon_policy\"\r\n      this.sendMessage(obj)\r\n    },\r\n    download: function() {\r\n      let dt = new Date()\r\n      let year = itoStr(dt.getFullYear())\r\n      let month = itoStr(dt.getMonth() + 1)\r\n      let day = itoStr(dt.getDate())\r\n      let hour = itoStr(dt.getHours())\r\n      let mins = itoStr(dt.getMinutes())\r\n      let postfix = year + \"_\" + month + \"_\" + day + \"_\" + hour + \"_\" + mins\r\n      const data = XLSX.utils.json_to_sheet(this.print_resistRocord)\r\n      const wb = XLSX.utils.book_new()\r\n      XLSX.utils.book_append_sheet(wb, data, \"data\")\r\n      XLSX.writeFile(wb, config.parking_name + \"_할인권 가격 정책_\" + postfix + \".xlsx\")\r\n    }\r\n  }\r\n}\r\n\r\nfunction itoStr($num) {\r\n  $num < 10 ? ($num = \"0\" + $num) : $num\r\n  return $num.toString()\r\n}\r\n</script>\r\n<style scoped>\r\n@import \"../assets/css/table.css\";\r\n@import \"../assets/css/divice.css\";\r\n</style>\r\n"],"sourceRoot":"src/views"}]}