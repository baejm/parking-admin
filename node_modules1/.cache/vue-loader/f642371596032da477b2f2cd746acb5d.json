<input\n              type=\"text\"\n              v-model=\"res.contents\"\n              v-on:change=\"contents_change(index)\"              \n          /></span>\n        </li>\n      </ul>\n    </div>\n  </div>\n</template>\n<script>\nimport config from \"../../config.js\"\nimport { mapState } from \"vuex\"\nexport default {\n  data() {\n    return {\n      resistIs:false,\n       reg: {\n        lp: \"\",\n        startTime: format_time1(new Date(Date.now())),\n        endTime:\"\" ,\n        name: \"\",\n        phone: \"\",\n        kind1: \"\",\n        contents: \"\",\n        number: \"\"\n      },\n       admin: [\n        {\n          order: \"\",\n          lp: \"\",\n          startTime: \"\",\n          endTime: \"\",\n          name: \"\",\n          phone: \"\",\n          kind1: \"\",\n          contents: \"\",\n          number: \"\",\n          check: true\n        }\n      ],\n      vehicle_obj_list: [\n        {\n          in_time: \"2020.09.14. 00:03:44\",\n          location: \"정문입구\",\n          direction: \"in\",\n          lp: \"서울86자5051\",\n          registered: \"등록차량\",\n          dong_ho: \" \",\n          contents: \"공적차량\"\n        },\n        {\n          in_time: \"2020.09.14. 00:03:44\",\n          location: \"정문입구\",\n          direction: \"in\",\n          lp: \"서울86자5051\",\n          registered: \"등록차량\",\n          dong_ho: \" \",\n          contents: \"공적차량\"\n        },\n        {\n          in_time: \"2020.09.14. 00:03:44\",\n          location: \"정문입구\",\n          direction: \"in\",\n          lp: \"서울86자5051\",\n          registered: \"등록차량\",\n          dong_ho: \" \",\n          contents: \"공적차량\"\n        },\n        {\n          in_time: \"2020.09.14. 00:03:44\",\n          location: \"정문입구\",\n          direction: \"in\",\n          lp: \"서울86자5051\",\n          registered: \"등록차량\",\n          dong_ho: \" \",\n          contents: \"공적차량\"\n        },\n        {\n          in_time: \"2020.09.14. 00:03:44\",\n     