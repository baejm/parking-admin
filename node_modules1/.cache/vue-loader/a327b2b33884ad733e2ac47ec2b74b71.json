             <button @click.prevent=\"item_name_click\">\r\n                  <font-awesome-icon\r\n                    icon=\"caret-down\"\r\n                    v-if=\"sort_item.name_click\"\r\n                  /><font-awesome-icon icon=\"caret-up\" v-else />\r\n                </button>\r\n              </th>\r\n\r\n              <th>\r\n                전화번호\r\n                <button @click.prevent=\"item_phone_click\">\r\n                  <font-awesome-icon\r\n                    icon=\"caret-down\"\r\n                    v-if=\"sort_item.phone_click\"\r\n                  /><font-awesome-icon icon=\"caret-up\" v-else />\r\n                </button>\r\n              </th>\r\n\r\n              <th>\r\n                구분\r\n                <button @click.prevent=\"item_kind1_click\">\r\n                  <font-awesome-icon\r\n                    icon=\"caret-down\"\r\n                    v-if=\"sort_item.kind1_click\"\r\n                  /><font-awesome-icon icon=\"caret-up\" v-else />\r\n                </button>\r\n              </th>\r\n\r\n              <th>\r\n                비고\r\n                <button @click.prevent=\"item_contents_click\">\r\n                  <font-awesome-icon\r\n                    icon=\"caret-down\"\r\n                    v-if=\"sort_item.contents_click\"\r\n                  /><font-awesome-icon icon=\"caret-up\" v-else />\r\n                </button>\r\n              </th>\r\n            </tr>\r\n          </thead>\r\n          <!-- 리스트 출력부분 -->\r\n          <tbody class=\"list-wrap\">\r\n            <tr\r\n              v-for=\"(ad, index) in resistRocord\"\r\n              :key=\"index\"\r\n              @click=\"modifyOpen(ad)\"\r\n            >\r\n              <td>{{ index + 1 }}</td>\r\n              <td>{{ ad.lp }}</td>\r\n              <td>{{ ad.name }}</td>\r\n              <td>{{ ad.phone }}</td>\r\n              <td>{{ ad.kind1 }}</td>\r\n              <td>{{ ad.contents }}</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport _ from \"underscore\"\r\nexport default {\r\n  data: function() {\r\n    return {\r\n      searchLp: \"\",\r\n      resistRocord: [],\r\n      admin: [\r\n        {\r\n          order: \"\",\r\n          lp: \"\",\r\n          name: \"\",\r\n          phone: \"\",\r\n          kind1: \"\",\r\n          contents: \"\",\r\n          number: \"\",\r\n          check: true\r\n        }\r\n      ],\r\n      current: {\r\n        lp: \"\",\r\n        name: \"\",\r\n        phone: \"\",\r\n        kind1: \"\",\r\n        number: \"\",\r\n        contents: \"\"\r\n      },\r\n      sort_item: {\r\n        lp_click: false,\r\n        name_click: false,\r\n        phone_click: false,\r\n        kind1_click: false,\r\n        contents_click: false\r\n      },\r\n      check: true,\r\n      resistIs: false,\r\n      modifyIs: false\r\n    }\r\n  },\r\n  created() {\r\n    this.$socket.on(\"message_from_server\", data => {\r\n      console.log(\"data.kind : \", data.kind)\r\n      this.special_vehicle_list(data)\r\n    }),\r\n      this.search1()\r\n  },\r\n  methods: {\r\n    registOpen() {\r\n      this.admin.lp = \"\"\r\n      this.admin.name = \"\"\r\n      this.admin.phone = \"\"\r\n      this.admin.kind1 = \"\"\r\n      this.admin.contents = \"\"\r\n      this.resistIs = true\r\n    },\r\n    registClose() {\r\n      this.resistIs = false\r\n    },\r\n    registNew(e) {\r\n      this.registClose()\r\n      e.preventDefault()\r\n      console.log(\"this.admin : \", this.admin)\r\n      let obj = {}\r\n      obj.lp = this.admin.lp\r\n      obj.name = this.admin.name\r\n      obj.phone = this.admin.phone\r\n      obj.company = this.admin.kind1\r\n      obj.contents = this.admin.contents\r\n      obj.content = this.admin.contents\r\n      obj.number = obj.lp.replace(/[가-힝]/gi, \"\")\r\n      obj.kind = \"create_special_vehicle_at_vehicle_list\"\r\n      console.log(\"obj :\", obj)\r\n      this.sendMessage(obj)\r\n    },\r\n    item_lp_click: function() {\r\n      console.log(\"lp sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.lp_click === false) {\r\n        console.log(\"lp sort1\")\r\n        this.sort_item.lp_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"lp\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"lp sort2\")\r\n        this.sort_item.lp_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"lp\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    item_name_click: function() {\r\n      console.log(\"name sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.name_click === false) {\r\n        console.log(\"name sort1\")\r\n        this.sort_item.name_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"name\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"name sort2\")\r\n        this.sort_item.name_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"name\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    item_phone_click: function() {\r\n      console.log(\"phone sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.phone_click === false) {\r\n        console.log(\"phone sort1\")\r\n        this.sort_item.phone_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"phone\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"phone sort2\")\r\n        this.sort_item.phone_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"phone\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    item_kind1_click: function() {\r\n      console.log(\"kind1 sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.kind1_click === false) {\r\n        console.log(\"kind1 sort1\")\r\n        this.sort_item.kind1_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"kind1\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"kind1 sort2\")\r\n        this.sort_item.kind1_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"kind1\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    item_contents_click: function() {\r\n      console.log(\"contents sort\")\r\n      let temp10 = []\r\n      if (this.sort_item.contents_click === false) {\r\n        console.log(\"contents sort1\")\r\n        this.sort_item.contents_click = true\r\n        temp10 = _.sortBy(this.resistRocord, \"contents\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        console.log(\"contents sort2\")\r\n        this.sort_item.contents_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"contents\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    search() {\r\n      console.log(\"검색 버튼을 눌렀습니다.\")\r\n      if (this.searchLp !== \"\" && this.search_dongHo != \"\") {\r\n        for (let i = 0; i < this.resistRocord.length; i++) {\r\n          if (\r\n            this.searchLp === this.resistRocord[i].lp &&\r\n            this.search_dongHo === this.resistRocord[i].dongHo\r\n          ) {\r\n            let c = this.resistRocord[i]\r\n            this.modifyOpen(c)\r\n            break\r\n          }\r\n        }\r\n      } else if (this.searchLp !== \"\") {\r\n        console.log(\"searchlp\")\r\n        for (let i = 0; i < this.resistRocord.length; i++) {\r\n          if (this.searchLp === this.resistRocord[i].lp) {\r\n            let c = this.resistRocord[i]\r\n            this.modifyOpen(c)\r\n            break\r\n          }\r\n        }\r\n      }\r\n    },\r\n    search1() {\r\n      let obj = {}\r\n      obj.kind = \"get_special_vehicle_list\"\r\n      this.sendMessage(obj)\r\n    },\r\n    special_vehicle_list(data) {\r\n      console.log(\"get_special_vehicle_list: \", data)\r\n      if (data.kind === \"get_special_vehicle_list\") {\r\n        this.resistRocord = []\r\n        for (var i = 0; i < data.docs.length; i++) {\r\n          data.docs[i].kind1 = data.docs[i].company\r\n          var obj = {}\r\n          obj.lp = data.docs[i].lp\r\n          obj.name = \"\"\r\n          if (data.docs[i].name) {\r\n            obj.name = data.docs[i].name\r\n          }\r\n          obj.phone = \"\"\r\n          if (data.docs[i].phone) {\r\n            obj.phone = data.docs[i].phone\r\n          }\r\n          obj.kind1 = \"\"\r\n          if (data.docs[i].kind1) {\r\n            obj.kind1 = data.docs[i].kind1\r\n          }\r\n          obj.contents = \"\"\r\n          if (data.docs[i].contents) {\r\n            obj.contents = data.docs[i].contents\r\n          }\r\n          obj.number = \"\"\r\n          if (data.docs[i].number) {\r\n            obj.number = data.docs[i].number\r\n          }\r\n          this.resistRocord.push(obj)\r\n        }\r\n        console.log(\"this.resistRocord : \", this.resistRocord)\r\n      } else if (\r\n        data.kind === \"create_special_vehicle_at_vehicle_list\" ||\r\n        data.kind === \"update_special_vehicle_at_vehicle_list\" ||\r\n        data.kind === \"delete_special_vehicle_at_vehicle_list\"\r\n      ) {\r\n        this.search1()\r\n      }\r\n    },\r\n    modifyOpen(c) {\r\n      this.modifyIs = true\r\n      this.current.lp = c.lp\r\n      this.current.name = c.name\r\n      this.current.phone = c.phone\r\n      this.current.kind1 = c.kind1\r\n      this.current.contents = c.contents\r\n      console.log(\"c : \", c.lp)\r\n      console.log(\"this : \", this)\r\n    },\r\n    modifyClose() {\r\n      this.modifyIs = false\r\n    },\r\n    searchItems() {\r\n      console.log(\"테스트\")\r\n    },\r\n    sendMessage(c) {\r\n      this.$socket.emit(\"message_from_web_client\", c)\r\n    },\r\n    modifyVehicle() {\r\n      console.log(\"차량정보 수정\")\r\n      this.modifyClose()\r\n      let obj = {}\r\n      obj.lp = this.current.lp\r\n      obj.name = this.current.name\r\n      obj.phone = this.current.phone\r\n      obj.kind1 = this.current.kind1\r\n      obj.company = obj.kind1\r\n      obj.contents = this.current.contents\r\n      obj.number = obj.lp.replace(/[가-힝]/gi, \"\")\r\n      obj.kind = \"update_special_vehicle_at_vehicle_list\"\r\n      this.sendMessage(obj)\r\n    },\r\n    deleteVehicle() {\r\n      console.log(\"차량정보 삭제\")\r\n      this.modifyClose()\r\n      let obj = {}\r\n      obj.lp = this.current.lp\r\n      obj.name = this.current.name\r\n      obj.phone = this.current.phone\r\n      obj.kind1 = this.current.kind1\r\n      obj.company = obj.kind1\r\n      obj.contents = this.current.contents\r\n      obj.number = obj.lp.replace(/[가-힝]/gi, \"\")\r\n      obj.kind = \"delete_special_vehicle_at_vehicle_list\"\r\n      this.sendMessage(obj)\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n.body_wrap {\r\n  height: auto;\r\n}\r\n.content_wrap {\r\n  height: 100%;\r\n}\r\n.content_wrap .menu_title {\r\n  display: flex;\r\n  margin-bottom: 15px;\r\n  height: 41px;\r\n}\r\n.content_wrap .menu_title form {\r\n  width: 100%;\r\n  display: flex;\r\n\r\n  -webkit-box-align: center;\r\n  -ms-flex-align: center;\r\n  align-items: center;\r\n}\r\n.content_wrap .menu_title form input {\r\n  border: 1px solid #c6c6c6;\r\n  margin-left: -1px;\r\n  padding: 10px 5px;\r\n  width: 18%;\r\n  height: 41px;\r\n}\r\n.content_wrap .menu_title form input[type=\"datetime-local\"] {\r\n  font-size: 14px;\r\n  padding: 9px 5px;\r\n}\r\n.content_wrap .menu_title form input::placeholder {\r\n  color: #bababa;\r\n  background: url(../assets/zoom_table.png) no-repeat 100% center;\r\n  padding: 0 20px 0 0px;\r\n}\r\n.content_wrap .menu_title form span:nth-child(3) {\r\n  margin: 0 5px;\r\n}\r\n\r\n.content_wrap .menu_title .seach_confirm {\r\n  col