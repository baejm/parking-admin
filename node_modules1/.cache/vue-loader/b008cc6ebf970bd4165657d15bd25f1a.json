uired />\r\n              <p>패스워드<span></span></p>\r\n              <input type=\"text\" v-model=\"current.residentPW\" />\r\n              <p>입주사명<span></span></p>\r\n              <input type=\"text\" v-model=\"current.company\" />\r\n              <p>이름<span></span></p>\r\n              <input type=\"text\" v-model=\"current.name\" />\r\n              <p>무료주차시간(분)<span></span></p>\r\n              <input type=\"text\" v-model=\"current.free_parking_time\" />\r\n              <p>전화번호<span></span></p>\r\n              <input type=\"text\" v-model=\"current.phone1\" />\r\n              <p>비고<span></span></p>\r\n              <input type=\"text\" v-model=\"current.contents\" />\r\n              <button @click.prevent=\"modifyVehicle\" class=\"modifybtn\">\r\n                수정\r\n              </button>\r\n              <button @click.prevent=\"deleteVehicle\" class=\"deletebtn\">\r\n                삭제\r\n              </button>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"list-items\">\r\n        <!-- 리스트 상단 메뉴 -->\r\n        <ul class=\"list-title\">\r\n          <li>\r\n            <span>\r\n              번호\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span>\r\n              입주사ID\r\n              <button @click.prevent=\"item_residentID_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.residentID_click\"\r\n                /><font-awesome-icon icon=\"caret-up\" v-else />\r\n              </button>\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span>\r\n              패스워드\r\n              <button @click.prevent=\"item_residentPW_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.residentPW_click\"\r\n                /><font-awesome-icon icon=\"caret-up\" v-else />\r\n              </button>\r\n          