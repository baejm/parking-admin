ACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"update_payment_terminal.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div id=\"sub_wrap\">\r\n    <div class=\"breadcrumbs\">\r\n      <ul>\r\n        <router-link :to=\"{ name: 'sub2_1' }\"> <li>Home</li></router-link>\r\n        <li>조회기능</li>\r\n        <li>무인정산기이용기록</li>\r\n      </ul>\r\n    </div>\r\n    <h1>무인정산기이용기록</h1>\r\n    <div class=\"content_wrap\">\r\n      <div class=\"menu_title\">\r\n        <form>\r\n          <span class=\"small_title\">날짜</span>\r\n          <input\r\n            type=\"datetime-local\"\r\n            id=\"start_time\"\r\n            v-model=\"start_time\"\r\n            placeholder=\"2020.03.20 오전 12:00\"\r\n          />\r\n          <span>~</span>\r\n          <input\r\n            type=\"datetime-local\"\r\n            id=\"end_time\"\r\n            v-model=\"end_time\"\r\n            placeholder=\"2020.03.20 오후 11:59\"\r\n          />\r\n          <span class=\"small_title\">차량번호</span>\r\n          <input\r\n            type=\"text\"\r\n            placeholder=\"차량 번호를 입력해 주세요.\"\r\n            v-model=\"searchNumber\"\r\n          />\r\n          <button class=\"seach_confirm\" @click.prevent=\"search\">검색</button>\r\n          <button @click.prevent=\"download\" class=\"savebtn\">저장</button>\r\n        </form>\r\n      </div>\r\n      <div class=\"list-items\">\r\n        <!-- 리스트 상단 메뉴 -->\r\n        <ul class=\"list-title\">\r\n          <li>\r\n            <span\r\n              >차량번호\r\n              <!-- <button @click.prevent=\"item_lp_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.lp\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button> -->\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >입차시각\r\n              <!-- <button @click.prevent=\"in_time_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.in_time\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >결재시각\r\n              <!-- <button @click.prevent=\"payment_time_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.payment_time\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >결재금액\r\n              <!-- <button @click.prevent=\"paid_fee_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.paid_fee\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >단말기 ID\r\n              <!-- <button @click.prevent=\"terminal_id_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.terminal_id\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >메모\r\n              <!-- <button @click.prevent=\"memo_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.memo\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li>\r\n            <span>차량사진</span>\r\n          </li>\r\n        </ul>\r\n        <!-- 리스트 출력부분 -->\r\n    <!--  <spinners v-if=\"isLoading\" /> \r\n        <ul class=\"list-wrap\" v-else> -->\r\n        <ul class=\"list-wrap\"> \r\n          <li v-for=\"(c, index) in vehicle_obj_list\" :key=\"index\">\r\n            <input type=\"text\" v-model=\"c.lp\" />\r\n            <input type=\"text\" v-model=\"c.in_time\" />\r\n            <input type=\"text\" v-model=\"c.payment_time\" />\r\n            <input type=\"text\" v-model=\"c.paid_fee\" />\r\n            <input type=\"text\" v-model=\"c.terminal_id\" />\r\n            <input\r\n              type=\"text\"\r\n              v-model=\"c.memo\"\r\n              v-on:change=\"contents_change(index)\"\r\n            />\r\n            <span\r\n              ><button @click.prevent=\"openImg(c)\">\r\n                <font-awesome-icon icon=\"camera-retro\" /></button\r\n            ></span>\r\n\r\n            <!--\t<button v-if=\"d.get_image_display\" @click.prevent=\"open_picture(d)\">보기<font-awesome-icon icon=\"angle-down\" /></button>\r\n\t\t\t\t\t\t<button v-else @click=\"close_picture(d)\">닫기<font-awesome-icon icon=\"angle-up\" /></button>\t-->\r\n            <div class=\"imageshow\" v-show=\"c.imageShow\">\r\n              <div>\r\n                <h3>차량 이미지</h3>\r\n                <button @click.prevent=\"closeImg(c)\">\r\n                  <font-awesome-icon icon=\"times-circle\" />\r\n                </button>\r\n                <img :src=\"c.imageName\" alt=\"\" />\r\n                <div>\r\n                  <span>차량번호 : {{ c.lp }}</span>\r\n                  <span>입차시각 : {{ c.time }}</span>\r\n                  <span>출차시각 : {{ c.out_time }} </span>\r\n                  <span>정산요금 : {{ c.paid_fee }} </span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </li>\r\n        </ul> \r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n//import spinners from \"../components/spinners\"\r\nimport XLSX from \"xlsx\"\r\nimport config from \"../config.js\"\r\nexport default {\r\n  data() {\r\n    return {\r\n      isLoading: false,\r\n      start_time: new Date(),\r\n      end_time: new Date(),\r\n      searchNumber: \"\",\r\n      btn: \"\",\r\n      vehicle_obj_list: [\r\n        //order: \"0\",\r\n        /*\ttime:  format_time1(new Date(1586297735243)),\r\n\t\t\t\t\tloop_event_time: format_time1(new Date(1586297735243)),\r\n\t\t\t\t\tlocation:\"입출구1\",\r\n\t\t\t\t\tlp:\"12가1234\",\r\n\t\t\t\t\tregistered: \"등록차량\",\r\n\t\t\t\t\tdong_ho: \"\",\r\n\t\t\t\t\timageName: require(\"../assets/car_test1.jpg\"),\r\n\t\t\t\t\timageShow: false */\r\n      ]\r\n    }\r\n  },\r\n  // components: {\r\n  //   spinners\r\n  // },\r\n  methods: {\r\n    socket_process(data) {\r\n   //   console.log(\"socket_process===>\", data)\r\n      if (data.kind === \"payment_result_list\") {\r\n        console.log(\"socket_process for ===> : \", data)\r\n        this.vehicle_obj_list = []       \r\n        for (var i = 0; i < data.docs.length; i++) {\r\n          let obj = data.docs[i]\r\n          if(obj.lp.indexOf(this.searchNumber) !== -1){\r\n            var obj1 = {}\r\n            obj1.lp = obj.lp\r\n            obj1.in_time = format_time1(new Date(obj.obj_in.loop_event_time))\r\n            obj1.payment_time = format_time1(new Date(obj.payment_time))\r\n            obj1.paid_fee = obj.paid_fee\r\n            if (!obj.terminal_id) {\r\n              obj1.terminal_id = \"01\"\r\n            }\r\n            obj1.memo = obj.contents\r\n            obj1.loop_event_time = obj.loop_event_time\r\n            obj1.imageName = config.image_src + obj.folder_name + \"/\" + obj.fname\r\n            obj1.imageShow = false\r\n            this.vehicle_obj_list.push(obj1)            \r\n          }\r\n          var total = 0\r\n          for (let i = 0; i < this.vehicle_obj_list.length; i++) {\r\n            total += this.vehicle_obj_list[i].paid_fee\r\n          }\r\n        }\r\n        let obj2 = {}\r\n        obj2.lp = \"합계\"\r\n        obj2.paid_fee = total\r\n        this.vehicle_obj_list.push(obj2)\r\n        this.isLoading = false\r\n        //console.log(this.vehicleInformation.length);\r\n       // console.log(this.vehicle_obj_list.length)\r\n       // console.log(\"this.vehicle_obj_list : \", this.vehicle_obj_list)        \r\n      }\r\n    },\r\n    listAdd() {\r\n      // this.dataList = this.inputList\r\n      this.dataList.unshift({\r\n        getTimer: this.listTimer,\r\n        getPc: this.listPc,\r\n        getPlace: this.listPlace,\r\n        getWay: this.listWay,\r\n        getCarNumer: this.listCarNumer,\r\n        getType: this.listType,\r\n        getLocation: this.listLocation,\r\n        getMemo: this.listMemo\r\n      })\r\n      this.listTimer = \"\"\r\n      this.listPc = \"\"\r\n      this.listPlace = \"\"\r\n      this.listWay = \"\"\r\n      this.listCarNumer = \"\"\r\n      this.listType = \"\"\r\n      this.listLocation = \"\"\r\n      this.listMemo = \"\"\r\n    },\r\n    date_set() {\r\n      let d = new Date()\r\n      //let d1 = d - (24*60*60*1000);\r\n      //console.log(\"d : \", d, \" d1 : \", d1);\r\n      var str_start =\r\n        d.getFullYear() +\r\n        \"-\" +\r\n        itoStr(d.getMonth() + 1) +\r\n        \"-\" +\r\n        itoStr(d.getDate()) +\r\n        \"T00:00\"\r\n      var str_end =\r\n        d.getFullYear() +\r\n        \"-\" +\r\n        itoStr(d.getMonth() + 1) +\r\n        \"-\" +\r\n        itoStr(d.getDate()) +\r\n        \"T23:59\"\r\n      this.start_time = str_start\r\n      this.end_time = str_end\r\n    },\r\n    contents_change(index) {\r\n      console.log(\"내용변경 :\", this.vehicle_obj_list[index])\r\n      var obj = {}\r\n      obj.kind = \"update_vehicle_obj_at_vehicle_obj_list\"\r\n      obj.lp = this.vehicle_obj_list[index].lp\r\n      obj.loop_event_time = this.vehicle_obj_list[index].loop_event_time\r\n      obj.contents = this.vehicle_obj_list[index].memo\r\n      this.sendMessage(obj)\r\n    },\r\n    sendMessage(c) {\r\n      console.log(\"this.sendMessage : \", c)\r\n      this.$socket.emit(\"message_from_web_client\", c)\r\n      this.isLoading = true\r\n    },\r\n    search() {\r\n      //this.vehicleInformation = [];\r\n      var obj = {}\r\n      obj.kind = \"payment_result_list\"\r\n      obj.lp = this.searchNumber\r\n      obj.start_time = get_start_time(this.start_time)\r\n      obj.end_time = get_end_time(this.end_time)\r\n      obj.id = window.localStorage.getItem(\"admin_id\")\r\n      obj.pw = window.localStorage.getItem(\"admin_pw\")\r\n      console.log(\"lp_search() : \", obj)\r\n      this.sendMessage(obj)\r\n    },\r\n    openImg(c) {\r\n      return (c.imageShow = true)\r\n\r\n      //console.log(\" openImg c : \", c);\r\n    },\r\n    closeImg(c) {\r\n      return (c.imageShow = false)\r\n      // console.log(\" closeImg c : \", c);\r\n    },\r\n    download : function() {\r\n      let dt = new Date();\r\n\t\t  let year = itoStr(dt.getFullYear())\r\n\t\t  let month = itoStr(dt.getMonth() + 1)\r\n\t\t  let day = itoStr(dt.getDate())\r\n\t\t  let hour = itoStr(dt.getHours())\r\n\t\t  let mins = itoStr(dt.getMinutes())\r\n      let postfix = year + \"_\" + month + \"_\" + day + \"_\" + hour + \"_\" + mins\r\n      const data = XLSX.utils.json_to_sheet(this.vehicle_obj_list)\r\n      const wb = XLSX.utils.book_new()\r\n      XLSX.utils.book_append_sheet(wb, data, \"data\")\r\n      XLSX.writeFile(wb, config.parking_name +\"_무인요금정산기록\" + postfix + \".xlsx\")\r\n    }\r\n  },\r\n  created() {\r\n    this.$socket.on(\"message_from_server\", data => {\r\n     // console.log(\"vehicle_obj_list data.kind : \", data)\r\n      this.socket_process(data)\r\n    })\r\n    this.date_set()\r\n  }\r\n}\r\n\r\nfunction format_time1(dat) {\r\n  var str = \"\"\r\n  str += dat.getFullYear()\r\n  str += \".\" + num_to_str(dat.getMonth() + 1)\r\n  str += \".\" + num_to_str(dat.getDate())\r\n  str += \". \" + num_to_str(dat.getHours())\r\n  str += \":\" + num_to_str(dat.getMinutes())\r\n  str += \":\" + num_to_str(dat.getSeconds())\r\n  return str\r\n}\r\n\r\nfunction num_to_str($num) {\r\n  $num < 10 ? ($num = \"0\" + $num) : $num\r\n  return $num.toString()\r\n}\r\n/*\r\nfunction get_datetime(str) {\r\n\tvar res = str.split(/[-T.:\\s]+/);\r\n\tvar d = new Date(parseInt(res[0]), parseInt(res[1])-1, parseInt(res[2]), parseInt(res[3]), parseInt(res[4]), 0, 0);\r\n\treturn d.getTime();\r\n} */\r\n\r\nfunction get_start_time(str) {\r\n  var res = str.split(/[-T.:\\s]+/)\r\n  var d = new Date(\r\n    parseInt(res[0]),\r\n    parseInt(res[1]) - 1,\r\n    parseInt(res[2]),\r\n    parseInt(res[3]),\r\n    parseInt(res[4]),\r\n    0,\r\n    0\r\n  )\r\n  return d.getTime()\r\n}\r\n\r\nfunction get_end_time(str) {\r\n  var res = str.split(/[-T.:\\s]+/)\r\n  var d = new Date(\r\n    parseInt(res[0]),\r\n    parseInt(res[1]) - 1,\r\n    parseInt(res[2]),\r\n    parseInt(res[3]),\r\n    parseInt(res[4]),\r\n    59,\r\n    999\r\n  )\r\n  return d.getTime()\r\n}\r\n\r\nfunction itoStr($num) {\r\n  $num < 10 ? ($num = \"0\" + $num) : $num\r\n  return $num.toString()\r\n}\r\n</script>\r\n<style scoped>\r\n.body_wrap {\r\n  height: auto;\r\n}\r\n.content_wrap {\r\n  height: 100%;\r\n}\r\n.content_wrap .menu_title {\r\n  display: flex;\r\n  margin-bottom: 15px;\r\n  height: 41px;\r\n}\r\n.content_wrap .menu_title button.savebtn {\r\n  background: #ffb300 !important;\r\n  margin-left: auto;\r\n}\r\n.content_wrap .menu_title button {\r\n  color: white;\r\n  margin-right: 5px;\r\n  background: #6a6cff !important;\r\n  width: 79px;\r\n  border-radius: 5px;\r\n  color: white;\r\n  height: 100%;\r\n  transition: all 0.2s ease;\r\n}\r\n.content_wrap .menu_title button:hover {\r\n  box-shadow: rgba(0, 0, 0, 0.2) 2px 2px 3px 0px inset;\r\n}\r\n.content_wrap .menu_title button:active {\r\n  box-shadow: 2px 1px 2px 