CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"update_discounted_vehicle_in_list.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div id=\"sub_wrap\">\r\n    <h1>방문차량할인(출차전)</h1>\r\n    <div class=\"content_wrap\">\r\n      <div class=\"menu_title\">\r\n        <form>\r\n          <span class=\"small_title\">날짜</span>\r\n          <input\r\n            type=\"datetime-local\"\r\n            id=\"start_time\"\r\n            v-model=\"start_time\"\r\n            placeholder=\"2020.03.20 오전 12:00\"\r\n          />\r\n          <span>~</span>\r\n          <input\r\n            type=\"datetime-local\"\r\n            id=\"end_time\"\r\n            v-model=\"end_time\"\r\n            placeholder=\"2020.03.20 오후 11:59\"\r\n          />\r\n          <span class=\"small_title\">차량번호</span>\r\n          <input\r\n            maxlength=\"9\"\r\n            type=\"text\"\r\n            placeholder=\"차량 번호를 입력해 주세요.\"\r\n            v-model=\"searchNumber\"\r\n          />\r\n          <button class=\"seach_confirm\" @click.prevent=\"search\">검색</button>\r\n        </form>\r\n      </div>\r\n      <div class=\"list-items\">\r\n        <ul class=\"list-title\">\r\n          <li>\r\n            <span>번호</span>\r\n          </li>\r\n          <li class=\"list-vehicle-number\">\r\n            <span\r\n              >차량번호<button @click.prevent=\"item_lp_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.lp\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li class=\"list-in-time\">\r\n            <span\r\n              >입차시각<button @click.prevent=\"in_time_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.in_time\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li class=\"list-confirm-time\">\r\n            <span\r\n              >방문자 확인시각<button @click.prevent=\"confirmTime_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.confirmTime\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li class=\"list-sale-time\">\r\n            <span\r\n              >방문자 할인시간<button\r\n                @click.prevent=\"discounted_duration_click\"\r\n              >\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.discounted_duration\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li class=\"list-resident-id\">\r\n            <span\r\n              >입주사 ID<button @click.prevent=\"id_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.id\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li class=\"list-resident-fee\">\r\n            <span\r\n              >입주사 부담금액<button @click.prevent=\"charge_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.charge\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li class=\"list-memo\">\r\n            <span\r\n              >메모<button @click.prevent=\"contents_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.contents\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li class=\"list-vehicle-pic\">\r\n            <span>차량사진</span>\r\n          </li>\r\n        </ul>\r\n        <ul class=\"list-wrap\">\r\n          <li v-for=\"(c, index) in residents\" :key=\"index\">\r\n            <span class=\"list-number\"> {{ index + 1 }}</span>\r\n            <input class=\"list-vehicle-number\" type=\"text\" v-model=\"c.lp\" />\r\n            <span class=\"list-out-time\">{{ c.time }}</span>\r\n            <span class=\"list-confirm-time\">{{ c.confirmTime }}</span>\r\n            <input\r\n              class=\"list-sale-time\"\r\n              type=\"text\"\r\n              v-model=\"c.discounted_duration\"\r\n            />\r\n            <input class=\"list-resident-id\" type=\"text\" v-model=\"c.id\" />\r\n            <input class=\"list-resident-pay\" type=\"text\" v-model=\"c.charge\" />\r\n            <input class=\"list-memo\" type=\"text\" v-model=\"c.contents\" />\r\n            <span class=\"list-vehicle-pic\"\r\n              ><button @click.prevent=\"openImg(c)\">\r\n                <font-awesome-icon icon=\"camera-retro\" /></button\r\n            ></span>\r\n\r\n            <div class=\"imageshow\" v-show=\"c.imageShow\">\r\n              <div>\r\n                <h3>차량 이미지</h3>\r\n                <button @click.prevent=\"closeImg(c)\">\r\n                  <font-awesome-icon icon=\"times-circle\" />\r\n                </button>\r\n                <img :src=\"c.image_name\" alt=\"\" />\r\n                <div>\r\n                  <span>차량번호 : {{ c.lp }}</span>\r\n                  <span>입차시각 : {{ c.time }}</span>\r\n                  <span>할인시간 : {{ c.discounted_duration }}</span>\r\n                  <span>입주사 부담금액 : {{ c.charge }}원</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport _ from \"underscore\"\r\nimport config from \"../config.js\"\r\nexport default {\r\n  data() {\r\n    return {\r\n      start_time: new Date(),\r\n      end_time: new Date(),\r\n      searchNumber: \"\",\r\n      btn: \"\",\r\n      residents: [\r\n        /*{\r\n\t\t\t\torder: \"0\",\r\n\t\t\t\tlp: \"29가1234\",\r\n\t\t\t\ttime:  format_time1(new Date(1586297735243)),\r\n\t\t\t\tconfirmTime: format_time1(new Date(1586297735243)),\r\n\t\t\t\tdiscounted_duration : \"60\",\r\n\t\t\t\tcharge: \"1000\",\r\n\t\t\t\timageName: require(\"../assets/car_test1.jpg\"),\r\n\t\t\t\timageShow: false\r\n\t\t\t\t}*/\r\n      ],\r\n      sort_item: {\r\n        lp: false,\r\n        in_time: false,\r\n        out_time: false,\r\n        confirmTime: false,\r\n        discounted_duration: false,\r\n        id: false,\r\n        charge: false\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    item_lp_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.lp === false) {\r\n        this.sort_item.lp = true\r\n        temp10 = _.sortBy(this.residents, \"lp\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.lp = false\r\n        temp10 = _.sortBy(this.residents, \"lp\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    contents_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.contents === false) {\r\n        this.sort_item.contents = true\r\n        temp10 = _.sortBy(this.contetns, \"lp\")\r\n        temp10.reverse()\r\n        return (this.contents = temp10)\r\n      } else {\r\n        this.sort_item.contents = false\r\n        temp10 = _.sortBy(this.contents, \"lp\")\r\n        return (this.contents = temp10)\r\n      }\r\n    },\r\n    in_time_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.in_time === false) {\r\n        this.sort_item.in_time = true\r\n        temp10 = _.sortBy(this.residents, \"in_time\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.in_time = false\r\n        temp10 = _.sortBy(this.residents, \"in_time\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    out_time_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.out_time === false) {\r\n        this.sort_item.out_time = true\r\n        temp10 = _.sortBy(this.residents, \"out_time\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.out_time = false\r\n        temp10 = _.sortBy(this.residents, \"out_time\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    confirmTime_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.confirmTime === false) {\r\n        this.sort_item.confirmTime = true\r\n        temp10 = _.sortBy(this.residents, \"confirmTime\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.confirmTime = false\r\n        temp10 = _.sortBy(this.residents, \"confirmTime\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    discounted_duration_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.discounted_duration === false) {\r\n        this.sort_item.discounted_duration = true\r\n        temp10 = _.sortBy(this.residents, \"discounted_duration\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.discounted_duration = false\r\n        temp10 = _.sortBy(this.residents, \"discounted_duration\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    id_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.id === false) {\r\n        this.sort_item.id = true\r\n        temp10 = _.sortBy(this.residents, \"id\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.id = false\r\n        temp10 = _.sortBy(this.residents, \"id\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    charge_click: function() {\r\n      let temp10 = []\r\n      if (this.sort_item.charge === false) {\r\n        this.sort_item.charge = true\r\n        temp10 = _.sortBy(this.residents, \"charge\")\r\n        temp10.reverse()\r\n        return (this.residents = temp10)\r\n      } else {\r\n        this.sort_item.charge = false\r\n        temp10 = _.sortBy(this.residents, \"charge\")\r\n        return (this.residents = temp10)\r\n      }\r\n    },\r\n    socket_process(data) {\r\n      // console.log(\"socket_process===>\", data)\r\n      if (\r\n        data.status === \"ok\" &&\r\n        data.kind === \"get_resident_discounted_list_admin\"\r\n      ) {\r\n        console.log(\"socket_process for ===> : \", data)\r\n        let vehicle_obj_list = []\r\n        for (let f = 0; f < data.docs.length; f++) {\r\n          let flag = false\r\n          for (let g = f + 1; g < data.docs.length; g++) {\r\n            if (data.docs[g].mark === true) {\r\n              console.log(\r\n                \"data.docs[f].mark === true ===> : \",\r\n                data.docs[f].mark\r\n              )\r\n              continue\r\n            }\r\n            if (data.docs[f].loop_event_time === data.docs[g].loop_event_time) {\r\n              flag = true\r\n              data.docs[g].mark = true\r\n            }\r\n          }\r\n          //    console.log(\"flag ===> : \", flag)\r\n          if (data.docs[f].mark !== true) {\r\n            if (flag === false) {\r\n              vehicle_obj_list.push(data.docs[f])\r\n            } else {\r\n              if (data.docs[f].lp !== \"미인식\") {\r\n                vehicle_obj_list.push(data.docs[f])\r\n                flag = false\r\n              }\r\n            }\r\n          }\r\n        }\r\n        console.log(\"svehicle_obj_list ===> : \", vehicle_obj_list)\r\n        for (var i = 0; i < vehicle_obj_list.length; i++) {\r\n          var obj1 = vehicle_obj_list[i]\r\n          for (var j = 0; j < obj1.discounted_list.length; j++) {\r\n            for (var k = j + 1; k < obj1.discounted_list.length; k++) {\r\n              if (obj1.discounted_list[j].id === obj1.discounted_list[k].id) {\r\n                obj1.discounted_list[k].splice(k, 1)\r\n              }\r\n            }\r\n            if (this.searchNumber === \"\") {\r\n              let obj2 = {}\r\n              obj2.image_name =\r\n                config.image_src + obj1.folder_name + \"/\" + obj1.fname\r\n              //console.log(\" obj2.image_name : \", obj2.image_name)\r\n              obj2.lp = obj1.lp\r\n              obj2.time = format_time1(new Date(obj1.loop_event_time))\r\n              obj2.confirmTime = format_time1(\r\n                new Date(obj1.discounted_list[j].discounted_time)\r\n              )\r\n              obj2.discounted_duration =\r\n                Math.ceil(\r\n                  parseInt(obj1.discounted_list[j].discounted_duration),\r\n                  10\r\n                ) /\r\n                (60 * 1000)\r\n              obj2.id = obj1.discounted_list[j].id\r\n              obj2.contents = obj1.contents\r\n              obj2.charge = \"\"\r\n              obj2.imageShow = false\r\n              this.residents.push(obj2)\r\n              break\r\n            } else {\r\n              if (obj1.lp.search(this.searchNumber) >= 0) {\r\n                let obj2 = {}\r\n                obj2.image_name =\r\n                  config.image_src + obj1.folder_name + \"/\" + obj1.fname\r\n                obj2.lp = obj1.lp\r\n                obj2.time = format_time1(new Date(obj1.loop_event_time))\r\n                obj2.confirmTime = format_time1(\r\n                  new Date(obj1.discounted_list[j].discounted_time)\r\n                )\r\n                obj2.discounted_duration =\r\n                  Math.ceil(\r\n                    parseInt(obj1.discounted_list[j].discounted_duration),\r\n                    10\r\n                  ) /\r\n                  (60 * 1000)\r\n                obj2.id = obj1.discounted_list[j].id\r\n                obj2.contents = obj1.contents\r\n                obj2.charge = \"\"\r\n                obj2.imageShow = false\r\n                this.residents.push(obj2)\r\n                break\r\n              }\r\n            }\r\n          }\r\n        }\r\n        //console.log(this.vehicleInformation.length);\r\n        console.log(this.residents.length)\r\n        console.log(this.residents)\r\n      }\r\n    },\r\n    listAdd() {\r\n      // this.dataList = this.inputList\r\n      this.dataList.unshift({\r\n        getTimer: this.listTimer,\r\n        getPc: this.listPc,\r\n        getPlace: this.listPlace,\r\n        getWay: this.listWay,\r\n        getCarNumer: this.listCarNumer,\r\n        getType: this.listType,\r\n        getLocation: this.listLocation,\r\n        getMemo: this.listMemo\r\n      })\r\n      this.listTimer = \"\"\r\n      this.listPc = \"\"\r\n      this.listPlace = \"\"\r\n      this.listWay = \"\"\r\n      this.listCarNumer = \"\"\r\n      this.listType = \"\"\r\n      this.listLocation = \"\"\r\n      this.listMemo = \"\"\r\n    },\r\n    date_set() {\r\n      let d = new Date()\r\n      //let d1 = d - (24*60*60*1000);\r\n      //console.log(\"d : \", d, \" d1 : \", d1);\r\n      var str_start =\r\n        d.getFullYear() +\r\n        \"-\" +\r\n        itoStr(d.getMonth() + 1) +\r\n        \"-\" +\r\n        itoStr(d.getDate()) +\r\n        \"T00:00\"\r\n      var str_end =\r\n        d.getFullYear() +\r\n        \"-\" +\r\n        itoStr(d.getMonth() + 1) +\r\n        \"-\" +\r\n        itoStr(d.getDate()) +\r\n        \"T23:59\"\r\n      this.start_time = str_start\r\n      this.end_time = str_end\r\n    },\r\n    sendMessage(c) {\r\n      this.$socket.emit(\"message_from_web_client\", c)\r\n    },\r\n    search() {\r\n      this.residents = []\r\n      var obj = {}\r\n      obj.kind = \"get_resident_discounted_list_admin\"\r\n      obj.lp = this.searchNumber\r\n      obj.start_time = get_start_time(this.start_time)\r\n      obj.end_time = get_end_time(this.end_time)\r\n      obj.id = window.localStorage.getItem(\"admin_id\")\r\n      obj.pw = window.localStorage.getItem(\"admin_pw\")\r\n      console.log(\"lp_search() : \", obj)\r\n      this.sendMessage(obj)\r\n    },\r\n    openImg(c) {\r\n      return (c.imageShow = true)\r\n      //console.log(\" openImg c : \", c);\r\n    },\r\n    closeImg(c) {\r\n      return (c.imageShow = false)\r\n      // console.log(\" closeImg c : \", c);\r\n    }\r\n  },\r\n  created() {\r\n    this.$socket.on(\"message_from_server\", data => {\r\n      console.log(\"resident_discount_data.kind : \", data)\r\n      this.socket_process(data)\r\n    })\r\n    this.date_set()\r\n    window.addEventListener(\"scroll\", this.handleScroll)\r\n  }\r\n}\r\n\r\nfunction format_time1(dat) {\r\n  var str = \"\"\r\n  str += dat.getFullYear()\r\n  str += \".\" + num_to_str(dat.getMonth() + 1)\r\n  str += \".\" + num_to_str(dat.getDate())\r\n  str += \". \" + num_to_str(dat.getHours())\r\n  str += \":\" + num_to_str(dat.getMinutes())\r\n  str += \":\" + num_to_str(dat.getSeconds())\r\n  return str\r\n}\r\n\r\nfunction num_to_str($num) {\r\n  $num < 10 ? ($num = \"0\" + $num) : $num\r\n  return $num.toString()\r\n}\r\n/*\r\nfunction get_datetime(str) {\r\n\tvar res = str.split(/[-T.:\\s]+/);\r\n\tvar d = new Date(parseInt(res[0]), parseInt(res[1])-1, parseInt(res[2]), parseInt(res[3]), parseInt(res[4]), 0, 0);\r\n\treturn d.getTime();\r\n} */\r\n\r\nfunction get_start_time(str) {\r\n  var res = str.split(/[-T.:\\s]+/)\r\n  var d = new Date(\r\n    parseInt(res[0]),\r\n    parseInt(res[1]) - 1,\r\n    parseInt(res[2]),\r\n    parseInt(res[3]),\r\n    parseInt(res[4]),\r\n    0,\r\n    0\r\n  )\r\n  return d.getTime()\r\n}\r\n\r\nfunction get_end_time(str) {\r\n  var res = str.split(/[-T.:\\s]+/)\r\n  var d = new Date(\r\n    parseInt(res[0]),\r\n    parseInt(res[1]) - 1,\r\n    parseInt(res[2]),\r\n    parseInt(res[3]),\r\n    parseInt(res[4]),\r\n    59,\r\n    999\r\n  )\r\n  return d.getTime()\r\n}\r\n\r\nfunction itoStr($num) {\r\n  $num < 10 ? ($num = \"0\" + $num) : $num\r\n  return $num.toString()\r\n}\r\n</script>\r\n<style scoped>\r\n@import \"../assets/css/table.css\";\r\n@import \"../assets/css/divice.css\";\r\n.content_wrap .menu_title button.savebtn {\r\n  margin-left: 0;\r\n}\r\n.list-resident-pay {\r\n  width: 70%;\r\n}\r\n.list-title .list-vehicle-pic {\r\n  margin-right: 21px;\r\n}\r\n</style>\r\n"]}]}