CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"update_fee_list.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div id=\"sub_wrap\">\r\n    <h1>정산기록</h1>\r\n    <div class=\"content_wrap\">\r\n      <div class=\"menu_title\">\r\n        <form>\r\n          <span class=\"small_title\">날짜</span>\r\n          <input\r\n            type=\"datetime-local\"\r\n            id=\"start_time\"\r\n            v-model=\"start_time\"\r\n            placeholder=\"2020.03.20 오전 12:00\"\r\n          />\r\n          <span>~</span>\r\n          <input\r\n            type=\"datetime-local\"\r\n            id=\"end_time\"\r\n            v-model=\"end_time\"\r\n            placeholder=\"2020.03.20 오후 11:59\"\r\n          />\r\n          <span class=\"small_title\">차량번호</span>\r\n          <input\r\n            type=\"text\"\r\n            placeholder=\"차량 번호를 입력해 주세요.\"\r\n            v-model=\"searchNumber\"\r\n          />\r\n          <button class=\"seach_confirm\" @click.prevent=\"search\">검색</button>\r\n          <button @click.prevent=\"download\" class=\"savebtn\">저장</button>\r\n        </form>\r\n      </div>\r\n      <div class=\"list-items\">\r\n        <ul class=\"list-title\">\r\n          <li class=\"list-in-time\">\r\n            <span\r\n              >입차시각\r\n              <!-- <button @click.prevent=\"in_time_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.in_time\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li class=\"list-out-time\">\r\n            <span\r\n              >출차시각\r\n              <!-- <button @click.prevent=\"out_time_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.out_time\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li class=\"list-type\">\r\n            <span\r\n              >구분\r\n              <!-- <button @click.prevent=\"registered_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.registered\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li class=\"list-vehicle-number\">\r\n            <span\r\n              >차량번호\r\n              <!-- <button @click.prevent=\"lp_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.lp\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else />\r\n              </button> -->\r\n            </span>\r\n          </li>\r\n          <li class=\"list-parking-fee\">\r\n            <span\r\n              >주차요금\r\n              <!-- <button @click.prevent=\"paid_fee_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.paid_fee\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li class=\"list-calculate-before\">\r\n            <span\r\n              >사전정산주차요금\r\n              <!-- <button @click.prevent=\"calculate_before_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.calculate_before\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li class=\"list-resident-fee\">\r\n            <span\r\n              >입주사부담요금\r\n              <!-- <button @click.prevent=\"resident_fee_click\">\r\n                 <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.resident_fee\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li class=\"list-resident-id\">\r\n            <span\r\n              >입주사ID\r\n              <!-- <button @click.prevent=\"id_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.id\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li class=\"list-payment-method\">\r\n            <span\r\n              >지불방법\r\n              <!-- <button @click.prevent=\"how_to_pay_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.how_to_pay\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n          <li class=\"list-memo\">\r\n            <span\r\n              >메모\r\n              <!-- <button @click.prevent=\"memo_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.memo\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            > -->\r\n            </span>\r\n          </li>\r\n        </ul>\r\n        <ul class=\"list-wrap\">\r\n          <li v-for=\"(c, index) in vehicle_obj_list\" :key=\"index\">\r\n            <input class=\"list-in-time\" type=\"text\" v-model=\"c.in_time\" />\r\n            <input class=\"list-out-time\" type=\"text\" v-model=\"c.out_time\" />\r\n            <input class=\"list-type\" type=\"text\" v-model=\"c.registered\" />\r\n            <input class=\"list-vehicle-number\" type=\"text\" v-model=\"c.lp\" />\r\n            <input class=\"list-parking-fee\" type=\"text\" v-model=\"c.fee\" />\r\n            <input class=\"list-parking-fee\" type=\"text\" v-model=\"c.paid_fee\" />\r\n            <input class=\"list-resident-fee\" type=\"text\" v-model=\"c.resident_fee\"/>\r\n            <input class=\"list-resident-id\" type=\"text\" v-model=\"c.resident_id\" />\r\n            <input class=\"list-payment-method\"  type=\"text\" v-model=\"c.how_to_pay\" />\r\n            <input class=\"list-memo\" type=\"text\" v-model=\"c.memo\" />\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport XLSX from \"xlsx\"\r\nexport default {\r\n  data() {\r\n    return {\r\n      start_time: new Date(),\r\n      end_time: new Date(),\r\n      searchNumber: \"\",\r\n      btn: \"\",\r\n      vehicle_obj_list: [\r\n        {\r\n          //order: \"0\",\r\n          /*time:  format_time1(new Date(1586297735243)),\r\n\t\t\t\t\tloop_event_time: format_time1(new Date(1586297735243)),\r\n\t\t\t\t\tlocation:\"입출구1\",\r\n\t\t\t\t\tlp:\"12가1234\",\r\n\t\t\t\t\tregistered: \"등록차량\",\r\n\t\t\t\t\tdong_ho: \"\",\r\n\t\t\t\t\timageName: require(\"../assets/car_test1.jpg\"),\r\n\t\t\t\t\timageShow: false */\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    socket_process(data) {\r\n      console.log(\"socket_process===>\", data)\r\n      if (data.kind === \"get_vehicle_obj_list\") {\r\n        console.log(\"socket_process for ===> : \", data)\r\n        this.vehicle_obj_list = []\r\n        for (var i = 0; i < data.docs.length; i++) {\r\n          let obj = data.docs[i]\r\n          if (\r\n            (obj.kind === \"lpr\" || obj.kind === \"fee_calculation_result\") &&\r\n            obj.direction === \"out\"\r\n          ) {\r\n            if(obj.lp.indexOf(this.searchNumber) !== -1){\r\n              if (obj.paid_fee === undefined) {\r\n                obj.paid_fee = 0\r\n              }\r\n              var obj1 = {}\r\n              var registered = obj.type\r\n              if (obj.type === undefined) {\r\n                registered = obj.registered\r\n              }\r\n\r\n              if (obj.obj_in) {\r\n                obj1.in_time = obj.obj_in.loop_event_time\r\n                if (obj.obj_in.discounted_list) {\r\n                  for (var j = 0; j < obj.obj_in.discounted_list.length; j++) {\r\n                    for (\r\n                      var k = j + 1;\r\n                      k < obj.obj_in.discounted_list.length;\r\n                      k++\r\n                    ) {\r\n                      // if (\r\n                      //   obj.obj_in.discounted_list[j].id ===\r\n                      //   obj.obj_in.discounted_list[k].id\r\n                      // ) {\r\n                      //   obj.discounted_list[k].splice(k, 1)\r\n                      // }\r\n                    }\r\n                    obj1.in_time = format_time1(\r\n                      new Date(obj.obj_in.loop_event_time)\r\n                    )\r\n                    obj1.out_time = format_time1(new Date(obj.loop_event_time))\r\n                    obj1.lp = obj.lp\r\n                    obj1.registered = registered\r\n                    obj1.fee = obj.fee\r\n                    obj1.paid_fee = obj.paid_fee\r\n                    obj1.resident_id = obj.obj_in.discounted_list[j].id\r\n                    obj1.resident_fee = obj.obj_in.discounted_list[j].resident_fee\r\n                    obj1.how_to_pay = obj.how_to_pay\r\n                    obj1.memo = obj.contents\r\n                    obj1.loop_event_time = obj.loop_event_time\r\n                    this.vehicle_obj_list.push(obj1)\r\n                  }\r\n                } else {\r\n                  obj1.in_time = format_time1(\r\n                    new Date(obj.obj_in.loop_event_time)\r\n                  )\r\n                  obj1.out_time = format_time1(new Date(obj.loop_event_time))\r\n                  obj1.lp = obj.lp\r\n                  obj1.registered = registered\r\n                  obj1.fee = obj.fee\r\n                  obj1.paid_fee = obj.paid_fee\r\n                  obj1.resident_id = \"\"\r\n                  obj1.resident_fee = 0\r\n                  obj1.how_to_pay = obj.how_to_pay\r\n                  obj1.memo = obj.contents\r\n                  obj1.loop_event_time = obj.loop_event_time\r\n                  this.vehicle_obj_list.push(obj1)\r\n                }\r\n              } else {\r\n                obj1.in_time = \"\"\r\n                obj1.out_time = format_time1(new Date(obj.loop_event_time))\r\n                obj1.lp = obj.lp\r\n                obj1.registered = registered\r\n                obj1.fee = obj.fee\r\n                obj1.paid_fee = obj.paid_fee\r\n                obj1.resident_id = \"\"\r\n                obj1.resident_fee = 0\r\n                obj1.how_to_pay = obj.how_to_pay\r\n                obj1.memo = obj.contents\r\n                obj1.loop_event_time = obj.loop_event_time\r\n                this.vehicle_obj_list.push(obj1)\r\n              }\r\n            }\r\n          }\r\n          //console.log(this.vehicleInformation.length);\r\n          console.log(this.vehicle_obj_list.length)\r\n          console.log(this.vehicle_obj_list)\r\n        }\r\n      }\r\n    },\r\n    listAdd() {\r\n      // this.dataList = this.inputList\r\n      this.dataList.unshift({\r\n        getTimer: this.listTimer,\r\n        getPc: this.listPc,\r\n        getPlace: this.listPlace,\r\n        getWay: this.listWay,\r\n        getCarNumer: this.listCarNumer,\r\n        getType: this.listType,\r\n        getLocation: this.listLocation,\r\n        getMemo: this.listMemo\r\n      })\r\n      this.listTimer = \"\"\r\n      this.listPc = \"\"\r\n      this.listPlace = \"\"\r\n      this.listWay = \"\"\r\n      this.listCarNumer = \"\"\r\n      this.listType = \"\"\r\n      this.listLocation = \"\"\r\n      this.listMemo = \"\"\r\n    },\r\n    date_set() {\r\n      let d = new Date()\r\n      //let d1 = d - (24*60*60*1000);\r\n      //console.log(\"d : \", d, \" d1 : \", d1);\r\n      var str_start =\r\n        d.getFullYear() +\r\n        \"-\" +\r\n        itoStr(d.getMonth() + 1) +\r\n        \"-\" +\r\n        itoStr(d.getDate()) +\r\n        \"T00:00\"\r\n      var str_end =\r\n        d.getFullYear() +\r\n        \"-\" +\r\n        itoStr(d.getMonth() + 1) +\r\n        \"-\" +\r\n        itoStr(d.getDate()) +\r\n        \"T23:59\"\r\n      this.start_time = str_start\r\n      this.end_time = str_end\r\n    },\r\n    sendMessage(c) {\r\n      this.$socket.emit(\"message_from_web_client\", c)\r\n    },\r\n    search() {\r\n      //this.vehicleInformation = [];\r\n      var obj = {}\r\n      obj.kind = \"get_vehicle_obj_list\"\r\n      obj.lp = this.searchNumber\r\n      obj.start_time = get_start_time(this.start_time)\r\n      obj.end_time = get_end_time(this.end_time)\r\n      obj.id = window.localStorage.getItem(\"id\")\r\n      obj.pw = window.localStorage.getItem(\"pw\")\r\n      console.log(\"lp_search() : \", obj)\r\n      this.sendMessage(obj)\r\n    },\r\n    download : function() {\r\n      let dt = new Date();\r\n\t\t  let year = itoStr(dt.getFullYear())\r\n\t\t  let month = itoStr(dt.getMonth() + 1)\r\n\t\t  let day = itoStr(dt.getDate())\r\n\t\t  let hour = itoStr(dt.getHours())\r\n\t\t  let mins = itoStr(dt.getMinutes())\r\n      let postfix = year + \"_\" + month + \"_\" + day + \"_\" + hour + \"_\" + mins\r\n      const data = XLSX.utils.json_to_sheet(this.vehicle_obj_list)\r\n      const wb = XLSX.utils.book_new()\r\n      XLSX.utils.book_append_sheet(wb, data, \"data\")\r\n      XLSX.writeFile(wb, \"하테크노타운주차장_ 정산기록_\" + postfix + \".xlsx\")\r\n    }\r\n  },\r\n  created() {\r\n    this.$socket.on(\"message_from_server\", data => {\r\n      console.log(\"vehicle_obj_list data.kind : \", data)\r\n      this.socket_process(data)\r\n    })\r\n    this.date_set()\r\n    window.addEventListener(\"scroll\", this.handleScroll)\r\n  }\r\n}\r\n\r\nfunction format_time1(dat) {\r\n  var str = \"\"\r\n  str += dat.getFullYear()\r\n  str += \".\" + num_to_str(dat.getMonth() + 1)\r\n  str += \".\" + num_to_str(dat.getDate())\r\n  str += \". \" + num_to_str(dat.getHours())\r\n  str += \":\" + num_to_str(dat.getMinutes())\r\n  str += \":\" + num_to_str(dat.getSeconds())\r\n  return str\r\n}\r\n\r\nfunction num_to_str($num) {\r\n  $num < 10 ? ($num = \"0\" + $num) : $num\r\n  return $num.toString()\r\n}\r\n/*\r\nfunction get_datetime(str) {\r\n\tvar res = str.split(/[-T.:\\s]+/);\r\n\tvar d = new Date(parseInt(res[0]), parseInt(res[1])-1, parseInt(res[2]), parseInt(res[3]), parseInt(res[4]), 0, 0);\r\n\treturn d.getTime();\r\n} */\r\n\r\nfunction get_start_time(str) {\r\n  var res = str.split(/[-T.:\\s]+/)\r\n  var d = new Date(\r\n    parseInt(res[0]),\r\n    parseInt(res[1]) - 1,\r\n    parseInt(res[2]),\r\n    parseInt(res[3]),\r\n    parseInt(res[4]),\r\n    0,\r\n    0\r\n  )\r\n  return d.getTime()\r\n}\r\n\r\nfunction get_end_time(str) {\r\n  var res = str.split(/[-T.:\\s]+/)\r\n  var d = new Date(\r\n    parseInt(res[0]),\r\n    parseInt(res[1]) - 1,\r\n    parseInt(res[2]),\r\n    parseInt(res[3]),\r\n    parseInt(res[4]),\r\n    59,\r\n    999\r\n  )\r\n  return d.getTime()\r\n}\r\n\r\nfunction itoStr($num) {\r\n  $num < 10 ? ($num = \"0\" + $num) : $num\r\n  return $num.toString()\r\n}\r\n</script>\r\n<style scoped>\r\n.body_wrap {\r\n  height: auto;\r\n}\r\n\r\n/* 테이블데이터 */\r\n.content_wrap {\r\n  height: 100%;\r\n}\r\n.content_wrap .menu_title {\r\n  display: flex;\r\n  margin-bottom: 15px;\r\n  height: 41px;\r\n}\r\n.content_wrap .menu_title button.savebtn {\r\n  background: #ffb300 !important;\r\n  margin-left: auto;\r\n}\