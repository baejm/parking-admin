text-field\r\n          class=\"id_info\"\r\n          v-model=\"admin_id\"\r\n          label=\"아이디를 입력하세요.\"\r\n          outlined\r\n          rounded\r\n          color=\"white\"\r\n        ></v-text-field>\r\n        <v-text-field\r\n          class=\"psw_info\"\r\n          v-model=\"admin_pw\"\r\n          label=\"비밀번호를 입력하세요.\"\r\n          outlined\r\n          rounded\r\n          type=\"password\"\r\n          color=\"white\"\r\n        ></v-text-field>\r\n        <v-btn\r\n          @click=\"login_check({ admin_id, admin_pw })\"\r\n          class=\"loginbtn\"\r\n          depressed=\"\"\r\n          block\r\n          rounded\r\n          color=\"#29a95c\"\r\n          ><span class=\"white--text\">Login</span></v-btn\r\n        >\r\n      </div>\r\n      <div class=\"callAs\">\r\n        <span background=\"black\" class=\"white--text\">대표번호</span>02-948-5400\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapActions } from \"vuex\"\r\nexport default {\r\n  data() {\r\n    return {\r\n      limitPosition: 0,\r\n      lastPosition: 300,\r\n      scrolled: false,\r\n      admin_id: \"\",\r\n      admin_pw: \"\",\r\n      admin_status: \"\"\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapState([\"loginSuccess\", \"loginError\", \"userGetDb\"])\r\n  },\r\n  methods: {\r\n    ...mapActions([\"login\", \"logout\"]),\r\n\r\n    freshOn() {\r\n      this.fresh = true\r\n    },\r\n    freshOff() {\r\n      this.fresh = false\r\n    },\r\n    login_check(admin_id) {\r\n      let obj = {}\r\n      obj.admin_id = admin_id.admin_id\r\n      obj.admin_pw = admin_id.admin_pw\r\n      obj.kind = \"login_admin\"\r\n      console.log(\"login_info : \", obj)\r\n      this.sendMessage(obj)\r\n    },\r\n    sendMessage(c) {\r\n      this.$socket.emit(\"message_from_web_client\", c)\r\n    },\r\n    socket_process(data){\r\n       if (data.kind === \"login_admin\" && data.status === \"ok\") {\r\n        console.log(\"login success\")\r\n        localStorage.admin_id = data.doc.id\r\n        localStorage.admin_pw = data.doc.pw\r\n        localStorage.admin_status = \"ok\"\r\n        let obj ={}\r\n        obj.admin_id = data.doc.id\r\n        obj.admin_pw = data.doc.pw\r\n        obj.admin_status = \"ok\"\r\n        this.$store.dispatch(\"login\", obj)\r\n      }\r\n    }\r\n    // menuHover(e) {\r\n    //   let y = e.clientY\r\n    //   let ele = document.getElementById(\"header\")\r\n    //   if (y < 100) {\r\n    //     ele.classList.add(\"headroom--down\")\r\n    //   }\r\n    // }\r\n  },\r\n  created() {\r\n    window.addEventListener(\"scroll\", this.handleScroll),\r\n    this.$socket.on(\"message_from_server\", data => {\r\n      console.log(\"data.kind : \", data)\r\n      this.socket_process(data)\r\n    })//,\r\n  //  \r\n  },\r\n  destroyed() {\r\n    window.removeEventListener(\"scroll\", this.handleScroll)\r\n  }\r\n}\r\n</script>\r\n<style>\r\n/* 로그인페이지 */\r\n.v-input input {\r\n  font-family: \"Noto Sans KR\";\r\n}\r\n.loginform_wrap label {\r\n  left: 30% !important;\r\n}\r\n.loginform_wrap .psw_info label {\r\n  left: 28% !important;\r\n}\r\n.v-text-field--outlined legend {\r\n  margin: 0 auto;\r\n}\r\n.loginform_wrap {\r\n  height: 100%;\r\n  width: 100%;\r\n  background-image: url(\"../assets/bg.jpg\");\r\n  background-size: cover;\r\n  min-height: 500px;\r\n}\r\n.loginform_wrap .content {\r\n  left: 50%;\r\n  width: 400px;\r\n  margin: 0 auto;\r\n  text-align: center;\r\n  position: absolute;\r\n  transform: translate(-50%, -50%);\r\n  top: 50%;\r\n}\r\n.input_button_wrap {\r\n  margin: 80px 0;\r\n}\r\n.loginbtn {\r\n  height: 56px !important;\r\n  font-size: 17px !important;\r\n  font-weight: bold !important;\r\n  color: white !important;\r\n}\r\n.wrap .content .logoImg {\r\n  margin-bottom: 80px;\r\n}\r\n.v-text-field.v-text-field--enclosed {\r\n  margin: 0 auto !important;\r\n}\r\n\r\n.v-input__slot {\r\n  width: 350px;\r\n}\r\n.callAs {\r\n  color: white;\r\n  font-weight: bold;\r\n  font-size: 21px;\r\n}\r\n.callAs span {\r\n  background: #202222;\r\n  margin-right: 10px;\r\n  font-weight: normal;\r\n  padding: 5px 7px;\r\n}\r\n</style>\r\n"]}]}