\r\n          <select v-model=\"searchDirection\">\r\n            <option\r\n              v-for=\"(direction1, index) in directions\"\r\n              v-bind:key=\"index\"\r\n            >\r\n              {{ direction1.text }}\r\n            </option>\r\n          </select>\r\n          <!-- <input\r\n            class=\"search_icon\"\r\n            type=\"text\"\r\n            maxlength=\"5\"\r\n            v-model=\"searchDirection\"\r\n          /> -->\r\n          <span class=\"small_title\">차량번호</span>\r\n          <input\r\n            class=\"search_icon\"\r\n            type=\"text\"\r\n            maxlength=\"9\"\r\n            v-model=\"seachNumber\"\r\n          />\r\n          <button @click.prevent=\"search\" class=\"seach_confirm\">검색</button>\r\n          <button @click.prevent=\"download\" class=\"savebtn\">저장</button>\r\n        </form>\r\n      </div>\r\n      <div class=\"list-items\">\r\n        <ul class=\"list-title\">\r\n          <li>\r\n            <span>번호</span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >차량출입시각<button @click.prevent=\"time_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.time_click\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >장소<button @click.prevent=\"location_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.location_click\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >방향<button @click.prevent=\"direction_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.direction_click\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >차량번호<button @click.prevent=\"item_lp_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.lp_click\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >구분<button @click.prevent=\"registered_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.registered\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >동호수<button @click.prevent=\"dong_ho_click\">\r\n                <font-awesome-icon icon=\"caret-down\" v-if=\"sort_item.dong_ho\" />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span\r\n              >메모<button @click.prevent=\"contents_click\">\r\n                <font-awesome-icon\r\n                  icon=\"caret-down\"\r\n                  v-if=\"sort_item.contents_click\"\r\n                />\r\n                <font-awesome-icon icon=\"caret-up\" v-else /></button\r\n            ></span>\r\n          </li>\r\n          <li>\r\n            <span>차량사진</span>\r\n          </li>\r\n        </ul>\r\n        <spinners v-if=\"isLoading\" />\r\n        <ul class=\"list-wrap\">\r\n          <li v-for=\"(res, index) in vehicle_obj_list\" :key=\"index\">\r\n            <span> {{ index + 1 }}</span>\r\n            <span class=\"time\"><input type=\"text\" v-model=\"res.time\"/></span>\r\n            <span class=\"location\"\r\n              ><input type=\"text\" v-model=\"res.location\"\r\n            /></span>\r\n            <span class=\"direction\"\r\n              ><input type=\"text\" v-model=\"res.direction\"\r\n            /></span>\r\n            <span class=\"lp\"\r\n              ><input\r\n                type=\"text\"\r\n                v-model=\"res.lp\"\r\n                v-on:change=\"contents_change(index)\"\r\n            /></span>\r\n            <span class=\"registered\"\r\n              ><input type=\"text\" v-model=\"res.registered\"\r\n            /></span>\r\n            <span class=\"dong_ho\"\r\n              ><input type=\"text\" v-model=\"res.dong_ho\"\r\n            /></span>\r\n            <span class=\"contents\"\r\n              ><input\r\n                type=\"text\"\r\n                v-model=\"res.contents\"\r\n                v-on:change=\"contents_change(index)\"\r\n            /></span>\r\n            <span class=\"picture_vichle\"\r\n              ><button @click=\"openImg(res)\">\r\n                <font-awesome-icon icon=\"camera-retro\" /></button\r\n            ></span>\r\n\r\n            <div class=\"imageshow\" v-show=\"res.imageShow\">\r\n              <div>\r\n                <h3>차량 이미지</h3>\r\n                <button @click=\"closeImg(res)\">\r\n                  <font-awesome-icon icon=\"times-circle\" />\r\n                </button>\r\n                <ul class=\"list-wrap\">\r\n                  <li v-for=\"(c, i) in res.imageList\" :key=\"i\">\r\n                    <img :src=\"res.imageList[i].imageName\" alt=\"\" />\r\n                  </li>\r\n                </ul>\r\n                <div>\r\n                  <span>차량번호 : {{ res.lp }}</span>\r\n                  <span>입차시각 : {{ res.time }}</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport _ from \"underscore\"\r\nimport XLSX from \"xlsx\"\r\nimpor