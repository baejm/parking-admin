 true\r\n        temp10 = _.sortBy(this.resistRocord, \"id\")\r\n        temp10.reverse()\r\n        return (this.resistRocord = temp10)\r\n      } else {\r\n        this.sort_item.id_click = false\r\n        temp10 = _.sortBy(this.resistRocord, \"id\")\r\n        return (this.resistRocord = temp10)\r\n      }\r\n    },\r\n    search() {\r\n      console.log(\"검색 버튼을 눌렀습니다.\", this.searchLp)\r\n      if (this.searchLp !== \"\" && this.searchID !== \"\") {\r\n        let temp_resistRocord = []\r\n        for (let i = 0; i < this.resistRocord.length; i++) {\r\n          if ((this.resistRocord[i].lp.indexOf(this.searchLp) !== -1) && (this.resistRocord[i].id.indexOf(this.searchID) !== -1)) {\r\n            temp_resistRocord.push(this.resistRocord[i])\r\n          }\r\n        }\r\n        this.resistRocord = temp_resistRocord\r\n      } else if(this.searchLp !== \"\") {\r\n        let temp_resistRocord = []\r\n        for (let i = 0; i < this.resistRocord.length; i++) {\r\n          if (this.resistRocord[i].lp.indexOf(this.searchLp) !== -1) {\r\n            temp_resistRocord.push(this.resistRocord[i])\r\n          }\r\n        }\r\n        this.resistRocord = temp_resistRocord\r\n      } else if(this.searchID !== \"\") {\r\n        let temp_resistRocord = []\r\n        for (let i = 0; i < this.resistRocord.length; i++) {\r\n          if (this.resistRocord[i].id.indexOf(this.searchID) !== -1) {\r\n            temp_resistRocord.push(this.resistRocord[i])\r\n          }\r\n        }\r\n        this.resistRocord = temp_resistRocord\r\n      } else {\r\n        this.search1()\r\n      }\r\n    },\r\n    search1() {\r\n      let obj = {}\r\n      obj.kind = \"get_visited_reserve_vehicle_list\"\r\n      this.sendMessage(obj)\r\n    },\r\n    sales_list(data) {\r\n      if (data.kind === \"get_visited_reserve_vehicle_list\") {\r\n        console.log(\"salse_list : \", data)\r\n        var last_time = {}\r\n        var sales_list = []\r\n\r\n        if (data.docs && data.docs.length > 0) {\r\n          for (let k = 0; k < data.docs.length; k++) {\r\n            if (data.docs[k].mark === true) {\r\n              continue\r\n            }\r\n            last_time = data.docs[k]\r\n            for (let l = k + 1; l < data.docs.length; l++) {\r\n              if (data.docs[k].lp === data.docs[l].lp) {\r\n                if (last_time.time < data.docs[l].time) {\r\n                  last_time = data.docs[l]\r\n                }\r\n                data.docs[l].mark = true\r\n              }\r\n            }\r\n            sales_list.push(last_time)\r\n          }\r\n          this.resistRocord = []\r\n          for (var i = 0; i < sales_list.length; i++) {\r\n            var obj = {}\r\n            obj.lp = sales_list[i].lp\r\n            obj.name = \" \"\r\n            if (sales_list[i].name) {\r\n              obj.name = sales_list[i].name\r\n            }\r\n            obj.phone = \" \"\r\n            if (sales_list[i].phone1) {\r\n              obj.phone = sales_list[i].phone1\r\n            }\r\n            obj.startTime = format_time1(new Date(0))\r\n            if (sales_list[i].period_start) {\r\n              obj.startTime = format_time1(new Date(sales_list[i].period_start))\r\n              obj.period_start = sales_list[i].period_start\r\n            }\r\n            obj.endTime = format_time1(new Date(0))\r\n            if (sales_list[i].period_end) {\r\n              obj.endTime = format_time1(new Date(sales_list[i].period_end))\r\n              obj.period_end = sales_list[i].period_end\r\n            }\r\n            obj.number = \" \"\r\n            if (sales_list[i].number) {\r\n              obj.number = sales_list[i].number\r\n            }\r\n            obj.kind1 = \" \"\r\n            if (sales_list[i].id) {\r\n              obj.resident_id = sales_list[i].id\r\n            }\r\n            obj.contents = \" \"\r\n            if (sales_list[i].contents) {\r\n              obj.contents = sales_list[i].contents\r\n            }\r\n\r\n            this.resistRocord.push(obj)\r\n          }\r\n          this.print_resistRocord = []\r\n          for(let i = 0; i < this.resistRocord.length; i++){\r\n            let print = {}\r\n            print.차량번호 = this.resistRocord[i].lp\r\n            print.개시시점 = this.resistRocord[i].startTime\r\n            print.종료시점 = this.resistRocord[i].endTime\r\n            print.이름 = this.resistRocord[i].name\r\n            print.연락처 = this.resistRocord[i].phone\r\n            print.입주사ID = this.resistRocord[i].resident_id\r\n            print.메모 = this.resistRocord[i].contents\r\n            this.print_resistRocord.push(print)\r\n          }\r\n\r\n          console.log(\"this.resistRocord : \", this.resistRocord)\r\n        }\r\n      }\r\n    },\r\n    modifyOpen(c) {\r\n      // this.modifyIs = true\r\n      // this.current.lp = c.lp\r\n      // this.current.name = c.name\r\n      // this.current.phone = c.phone\r\n      // this.current.startTime = c.startTime\r\n      // this.current.endTime = c.endTime\r\n      // this.current.kind1 = c.kind1\r\n      // this.current.contents = c.contents\r\n      this.$refs.btn.focus()\r\n      console.log(\"c : \", c.lp)\r\n      console.log(\"this : \", this)\r\n    },\r\n    modifyClose() {\r\n      this.modifyIs = false\r\n    },\r\n    searchItems() {\r\n      console.log(\"테스트\")\r\n    },\r\n    sendMessage(c) {\r\n      this.$socket.emit(\"message_from_web_client\", c)\r\n    },\r\n    modifyVehicle() {\r\n      console.log(\"차량정보 수정\")\r\n      this.modifyClose()\r\n      let obj = {}\r\n      obj.lp = this.current.lp\r\n      obj.name = this.current.name\r\n      obj.phone1 = this.current.phone\r\n      obj.period_start = get_datetime(this.current.startTime)\r\n      obj.period_end = get_datetime(this.current.endTime)\r\n      obj.contents = this.current.contents\r\n      obj.id = this.current.resident_id\r\n      obj.number = obj.lp.replace(/[가-힝]/gi, \"\")\r\n      obj.kind = \"update_sales_at_sales_list\"\r\n      console.log(\"차량정보수정 :\", obj)\r\n      this.sendMessage(obj)\r\n    },\r\n    deleteVehicle() {\r\n      console.log(\"차량정보 삭제\")\r\n      this.modifyClose()\r\n      let obj = {}\r\n\r\n      obj.lp = this.current.lp\r\n      obj.name = this.current.name\r\n      obj.phone1 = this.current.phone\r\n      obj.period_start = this.current.period_start\r\n      obj.period_end = this.current.eperiod_end\r\n      obj.contents = this.current.contents\r\n      obj.id = this.current.resident_id\r\n      obj.number = obj.lp.replace(/[가-힝]/gi, \"\")\r\n      obj.kind = \"delete_sales_at_sales_list\"\r\n      this.sendMessage(obj)\r\n    },\r\n    download: function() {\r\n      let dt = new Date()\r\n      let year = itoStr(dt.getFullYear())\r\n      let month = itoStr(dt.getMonth() + 1)\r\n      let day = itoStr(dt.getDate())\r\n      let hour = itoStr(dt.getHours())\r\n      let mins = itoStr(dt.getMinutes())\r\n      let postfix = year + \"_\" + month + \"_\" + day + \"_\" + hour + \"_\" + mins\r\n      const data = XLSX.utils.json_to_sheet(this.print_resistRocord)\r\n      const wb = XLSX.utils.book_new()\r\n      XLSX.utils.book_append_sheet(wb, data, \"data\")\r\n      XLSX.writeFile(wb, config.parking_name + \"_방문 예약 차량 기록 조회\" + postfix + \".xlsx\")\r\n    }\r\n  }\r\n}\r\n\r\nfunction format_time1(dat) {\r\n  var str = \"\"\r\n  str += dat.getFullYear()\r\n  str += \".\" + num_to_str(dat.getMonth() + 1)\r\n  str += \".\" + num_to_str(dat.getDate())\r\n  str += \". \" + num_to_str(dat.getHours())\r\n  str += \":\" + num_to_str(dat.getMinutes())\r\n  str += \":\" + num_to_str(dat.getSeconds())\r\n  return str\r\n}\r\n\r\nfunction num_to_str($num) {\r\n  $num < 10 ? ($num = \"0\" + $num) : $num\r\n  return $num.toString()\r\n}\r\n\r\nfunction get_datetime(str) {\r\n  var res = str.split(/[-T.:\\s]+/)\r\n  var d = new Date(\r\n    parseInt(res[0]),\r\n    parseInt(res[1]) - 1,\r\n    parseInt(res[2]),\r\n    parseInt(res[3]),\r\n    parseInt(res[4]),\r\n    0,\r\n    0\r\n  )\r\n  return d.getTime()\r\n}\r\nfunction itoStr($num) {\r\n  $num < 10 ? ($num = \"0\" + $num) : $num\r\n  return $num.toString()\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import \"../assets/css/table.css\";\r\n@import \"../assets/css/divice.css\";\r\n.content_wrap .menu_title button.savebtn {\r\n  margin-left: auto;\r\n}\r\n</style>\r\n"]}]}